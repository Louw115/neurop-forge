  "logic": "def caesar_cipher(text: str, shift: int) -> str:\n    \"\"\"Apply Caesar cipher.\"\"\"\n    result = []\n    for c in text:\n        if \"a\" <= c <= \"z\":\n            result.append(chr((ord(c) - ord(\"a\") + shift) % 26 + ord(\"a\")))\n        elif \"A\" <= c <= \"Z\":\n            result.append(chr((ord(c) - ord(\"A\") + shift) % 26 + ord(\"A\")))\n        else:\n            result.append(c)\n    return \"\".join(result)",
