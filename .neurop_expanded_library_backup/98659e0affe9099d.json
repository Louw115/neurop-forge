  "logic": "def smoothstep(edge0: float, edge1: float, x: float) -> float:\n    \"\"\"Smooth step interpolation.\"\"\"\n    t = clamp((x - edge0) / (edge1 - edge0), 0, 1)\n    return t * t * (3 - 2 * t)",
