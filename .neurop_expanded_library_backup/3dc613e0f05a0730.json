  "logic": "def parse_ratio(ratio_str: str) -> tuple:\n    \"\"\"Parse ratio from string.\"\"\"\n    for sep in [\":\", \"/\", \"-\"]:\n        if sep in ratio_str:\n            parts = ratio_str.split(sep)\n            if len(parts) == 2:\n                try:\n                    return create_ratio(int(parts[0]), int(parts[1]))\n                except ValueError:\n                    pass\n    return (0, 0)",
