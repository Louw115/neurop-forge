  "logic": "def format_duration_human(seconds: int) -> str:\n    \"\"\"Format duration in human-readable form.\"\"\"\n    if seconds < 60:\n        return f\"{seconds}s\"\n    if seconds < 3600:\n        return f\"{seconds // 60}m {seconds % 60}s\"\n    hours = seconds // 3600\n    minutes = (seconds % 3600) // 60\n    return f\"{hours}h {minutes}m\"",
