  "logic": "def sanitize_filename(filename: str) -> str:\n    \"\"\"Sanitize a filename by removing unsafe characters.\"\"\"\n    if not filename:\n        return \"\"\n    unsafe = '<>:\"/\\\\|?*\\x00'\n    result = ''.join(c for c in filename if c not in unsafe)\n    result = result.strip('. ')\n    return result or 'unnamed'",
