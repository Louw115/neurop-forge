  "logic": "def compare_semver(v1: str, v2: str) -> int:\n    \"\"\"Compare two semantic versions. Returns -1, 0, or 1.\"\"\"\n    p1 = parse_semver(v1)\n    p2 = parse_semver(v2)\n    for key in [\"major\", \"minor\", \"patch\"]:\n        if p1[key] < p2[key]:\n            return -1\n        if p1[key] > p2[key]:\n            return 1\n    return 0",
