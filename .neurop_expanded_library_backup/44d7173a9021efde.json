  "logic": "def assert_in_range(value, min_val, max_val) -> dict:\n    \"\"\"Assert value is in range.\"\"\"\n    passed = min_val <= value <= max_val\n    return {\n        \"passed\": passed,\n        \"actual\": value,\n        \"min\": min_val,\n        \"max\": max_val,\n        \"message\": \"\" if passed else f\"Expected {value} in [{min_val}, {max_val}]\"\n    }",
