  "logic": "def mask_contact_email(contact: dict) -> dict:\n    \"\"\"Mask email in contact.\"\"\"\n    email = contact.get(\"email\", \"\")\n    if \"@\" in email:\n        local, domain = email.rsplit(\"@\", 1)\n        masked = local[0] + \"***\" + \"@\" + domain\n    else:\n        masked = email\n    return {**contact, \"email\": masked}",
