  "logic": "def is_valid_expression(expr: str) -> bool:\n    \"\"\"Check if expression is syntactically valid.\"\"\"\n    tokens = tokenize_expression(expr)\n    paren_count = 0\n    prev_type = None\n    for token in tokens:\n        if token[\"value\"] == \"(\":\n            paren_count += 1\n        elif token[\"value\"] == \")\":\n            paren_count -= 1\n        if paren_count < 0:\n            return False\n        if token[\"type\"] == \"operator\" and prev_type == \"operator\":\n            return False\n        prev_type = token[\"type\"]\n    return paren_count == 0",
