  "logic": "def contains_xss_patterns(text: str) -> bool:\n    \"\"\"Check if text contains common XSS patterns.\"\"\"\n    if not text:\n        return False\n    patterns = [\n        r'<script',\n        r'javascript:',\n        r'onerror\\s*=',\n        r'onload\\s*=',\n        r'onclick\\s*=',\n        r'onmouseover\\s*=',\n        r'onfocus\\s*=',\n        r'onblur\\s*=',\n    ]\n    text_lower = text.lower()\n    for pattern in patterns:\n        if re.search(pattern, text_lower, re.IGNORECASE):\n            return True\n    return False",
