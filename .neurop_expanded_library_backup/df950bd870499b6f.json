  "logic": "def lru_remove(cache: dict, key: str) -> dict:\n    \"\"\"Remove key from cache.\"\"\"\n    if key not in cache[\"items\"]:\n        return cache\n    items = dict(cache[\"items\"])\n    del items[key]\n    order = [k for k in cache[\"order\"] if k != key]\n    return {\n        \"capacity\": cache[\"capacity\"],\n        \"items\": items,\n        \"order\": order,\n        \"size\": len(items)\n    }",
