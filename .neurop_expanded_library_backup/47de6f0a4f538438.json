  "logic": "def get_current_breakpoint(width: int, breakpoints: dict) -> str:\n    \"\"\"Get current breakpoint name for width.\"\"\"\n    current = \"xs\"\n    for name, min_width in sorted(breakpoints.items(), key=lambda x: x[1]):\n        if width >= min_width:\n            current = name\n    return current",
