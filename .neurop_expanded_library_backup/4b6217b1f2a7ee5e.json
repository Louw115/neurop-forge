  "logic": "def parse_error_code(error_code: str) -> dict:\n    \"\"\"Parse error code into components.\"\"\"\n    parts = error_code.split(\"-\")\n    if len(parts) != 2:\n        return {\"valid\": False, \"category\": \"\", \"code\": 0}\n    try:\n        return {\n            \"valid\": True,\n            \"category\": parts[0],\n            \"code\": int(parts[1])\n        }\n    except ValueError:\n        return {\"valid\": False, \"category\": \"\", \"code\": 0}",
