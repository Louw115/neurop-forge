  "logic": "def normalize_mac_address(mac: str, separator: str) -> str:\n    \"\"\"Normalize MAC address format.\"\"\"\n    clean = mac.replace(\":\", \"\").replace(\"-\", \"\").lower()\n    if len(clean) != 12:\n        return mac\n    return separator.join(clean[i:i+2] for i in range(0, 12, 2))",
