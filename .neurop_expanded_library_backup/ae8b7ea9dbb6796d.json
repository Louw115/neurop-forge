  "logic": "def html_encode(text: str) -> str:\n    \"\"\"HTML encode a string.\"\"\"\n    return (text\n        .replace(\"&\", \"&amp;\")\n        .replace(\"<\", \"&lt;\")\n        .replace(\">\", \"&gt;\")\n        .replace('\"', \"&quot;\")\n        .replace(\"'\", \"&#x27;\"))",
