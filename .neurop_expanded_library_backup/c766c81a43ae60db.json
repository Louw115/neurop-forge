  "logic": "def get_triadic_colors(r: int, g: int, b: int) -> list:\n    \"\"\"Get triadic colors (three evenly spaced colors).\"\"\"\n    h, s, l = rgb_to_hsl(r, g, b)\n    return [\n        hsl_to_rgb(h, s, l),\n        hsl_to_rgb((h + 120) % 360, s, l),\n        hsl_to_rgb((h + 240) % 360, s, l),\n    ]",
