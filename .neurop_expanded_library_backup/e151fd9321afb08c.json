  "logic": "def compact_deep(obj: dict) -> dict:\n    \"\"\"Recursively remove None values.\"\"\"\n    result = {}\n    for k, v in obj.items():\n        if v is None:\n            continue\n        if isinstance(v, dict):\n            v = compact_deep(v)\n        result[k] = v\n    return result",
