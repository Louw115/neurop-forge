  "logic": "def has_nested_key(obj: dict, path: str) -> bool:\n    \"\"\"Check if nested key exists.\"\"\"\n    keys = path.split(\".\")\n    current = obj\n    for key in keys:\n        if isinstance(current, dict) and key in current:\n            current = current[key]\n        else:\n            return False\n    return True",
