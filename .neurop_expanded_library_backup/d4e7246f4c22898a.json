  "logic": "def get_cache_stats(hits: int, misses: int) -> dict:\n    \"\"\"Calculate cache statistics.\"\"\"\n    total = hits + misses\n    return {\n        \"hits\": hits,\n        \"misses\": misses,\n        \"total\": total,\n        \"hit_rate\": hits / total if total > 0 else 0,\n        \"miss_rate\": misses / total if total > 0 else 0\n    }",
