  "logic": "def get_char_name(char: str) -> str:\n    \"\"\"Get name of ASCII character.\"\"\"\n    code = ord(char)\n    names = {\n        0: \"NUL\", 7: \"BEL\", 8: \"BS\", 9: \"TAB\", 10: \"LF\",\n        13: \"CR\", 27: \"ESC\", 32: \"SPACE\", 127: \"DEL\"\n    }\n    if code in names:\n        return names[code]\n    if is_printable_ascii(char):\n        return char\n    return f\"0x{code:02X}\"",
