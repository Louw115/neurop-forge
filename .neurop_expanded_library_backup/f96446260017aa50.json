  "logic": "def dedent_text(text: str, spaces: int) -> str:\n    \"\"\"Remove indentation from each line.\"\"\"\n    lines = []\n    for line in text.split(\"\\n\"):\n        if line.startswith(\" \" * spaces):\n            lines.append(line[spaces:])\n        else:\n            lines.append(line.lstrip())\n    return \"\\n\".join(lines)",
