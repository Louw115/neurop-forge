  "logic": "def parse_cookie_header(header: str) -> dict:\n    \"\"\"Parse a Cookie header into a dictionary.\"\"\"\n    if not header:\n        return {}\n    result = {}\n    pairs = header.split(';')\n    for pair in pairs:\n        if '=' in pair:\n            key, value = pair.split('=', 1)\n            result[key.strip()] = value.strip()\n    return result",
