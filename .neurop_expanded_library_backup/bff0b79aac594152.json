  "logic": "def map_range(value: float, in_min: float, in_max: float, out_min: float, out_max: float) -> float:\n    \"\"\"Map a value from one range to another.\"\"\"\n    if in_max == in_min:\n        return out_min\n    return (value - in_min) * (out_max - out_min) / (in_max - in_min) + out_min",
