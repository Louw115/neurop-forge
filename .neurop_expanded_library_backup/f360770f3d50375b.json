  "logic": "def parse_content_type(header: str) -> dict:\n    \"\"\"Parse Content-Type header.\"\"\"\n    parts = header.split(\";\")\n    mime_type = parts[0].strip()\n    params = {}\n    for part in parts[1:]:\n        if \"=\" in part:\n            key, value = part.strip().split(\"=\", 1)\n            params[key.strip()] = value.strip().strip('\"')\n    return {\"mime_type\": mime_type, \"params\": params}",
