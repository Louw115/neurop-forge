  "logic": "def hsv_to_rgb(h: int, s: int, v: int) -> dict:\n    \"\"\"Convert HSV to RGB.\"\"\"\n    h_norm = h / 360\n    s_norm = s / 100\n    v_norm = v / 100\n    i = int(h_norm * 6)\n    f = h_norm * 6 - i\n    p = v_norm * (1 - s_norm)\n    q = v_norm * (1 - f * s_norm)\n    t = v_norm * (1 - (1 - f) * s_norm)\n    i %= 6\n    if i == 0:\n        r, g, b = v_norm, t, p\n    elif i == 1:\n        r, g, b = q, v_norm, p\n    elif i == 2:\n        r, g, b = p, v_norm, t\n    elif i == 3:\n        r, g, b = p, q, v_norm\n    elif i == 4:\n        r, g, b = t, p, v_norm\n    else:\n        r, g, b = v_norm, p, q\n    return {\"r\": round(r * 255), \"g\": round(g * 255), \"b\": round(b * 255)}",
