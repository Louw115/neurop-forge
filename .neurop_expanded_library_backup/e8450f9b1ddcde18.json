  "logic": "def calculate_npv(cash_flows: list, discount_rate: float) -> float:\n    \"\"\"Calculate net present value.\"\"\"\n    npv = 0.0\n    for i, cf in enumerate(cash_flows):\n        npv += cf / ((1 + discount_rate / 100) ** i)\n    return npv",
