  "logic": "def to_float(value) -> float:\n    \"\"\"Convert a value to a float.\"\"\"\n    if value is None:\n        return 0.0\n    if isinstance(value, bool):\n        return 1.0 if value else 0.0\n    if isinstance(value, (int, float)):\n        return float(value)\n    if isinstance(value, str):\n        try:\n            return float(value)\n        except ValueError:\n            return 0.0\n    return 0.0",
