  "logic": "def unflatten_dict(data: dict, separator: str) -> dict:\n    \"\"\"Unflatten a flat dictionary into a nested dictionary.\"\"\"\n    if not data:\n        return {}\n    if not separator:\n        separator = '.'\n    result = {}\n    for flat_key, value in data.items():\n        result = set_nested_value(result, flat_key, value, separator)\n    return result",
