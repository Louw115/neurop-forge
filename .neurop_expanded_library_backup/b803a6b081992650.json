  "logic": "def top_words(text: str, n: int) -> list:\n    \"\"\"Get top n most frequent words.\"\"\"\n    freq = get_word_frequency(text)\n    sorted_words = sorted(freq.items(), key=lambda x: -x[1])\n    return sorted_words[:n]",
