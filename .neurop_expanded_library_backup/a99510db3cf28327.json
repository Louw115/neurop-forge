  "logic": "def count_events_by_type(events: list) -> dict:\n    \"\"\"Count events by type.\"\"\"\n    counts = {}\n    for event in events:\n        event_type = event.get(\"type\", \"unknown\")\n        counts[event_type] = counts.get(event_type, 0) + 1\n    return counts",
