  "logic": "def escape_html(text: str) -> str:\n    \"\"\"Escape HTML special characters in a string.\"\"\"\n    if not text:\n        return \"\"\n    return (text\n            .replace('&', '&amp;')\n            .replace('<', '&lt;')\n            .replace('>', '&gt;')\n            .replace('\"', '&quot;')\n            .replace(\"'\", '&#39;'))",
