  "logic": "def build_redis_key(prefix: str, parts: list, separator: str) -> str:\n    \"\"\"Build a Redis key from parts.\"\"\"\n    all_parts = [prefix] + [str(p) for p in parts]\n    return separator.join(all_parts)",
