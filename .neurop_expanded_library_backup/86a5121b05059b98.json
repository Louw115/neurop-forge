  "logic": "def matrix_add(a: list, b: list) -> list:\n    \"\"\"Add two matrices.\"\"\"\n    if not a or not b:\n        return []\n    if len(a) != len(b) or len(a[0]) != len(b[0]):\n        return []\n    return [[a[i][j] + b[i][j] for j in range(len(a[0]))] for i in range(len(a))]",
