  "logic": "def longest_common_substring(s1: str, s2: str) -> str:\n    \"\"\"Find longest common substring.\"\"\"\n    if not s1 or not s2:\n        return \"\"\n    m, n = len(s1), len(s2)\n    dp = [[0] * (n + 1) for _ in range(m + 1)]\n    max_len = 0\n    end_pos = 0\n    for i in range(1, m + 1):\n        for j in range(1, n + 1):\n            if s1[i-1] == s2[j-1]:\n                dp[i][j] = dp[i-1][j-1] + 1\n                if dp[i][j] > max_len:\n                    max_len = dp[i][j]\n                    end_pos = i\n    return s1[end_pos-max_len:end_pos]",
