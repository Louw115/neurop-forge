  "logic": "def compass_direction(bearing: float) -> str:\n    \"\"\"Convert bearing to compass direction.\"\"\"\n    directions = ['N', 'NNE', 'NE', 'ENE', 'E', 'ESE', 'SE', 'SSE', \n                  'S', 'SSW', 'SW', 'WSW', 'W', 'WNW', 'NW', 'NNW']\n    index = round(bearing / 22.5) % 16\n    return directions[index]",
