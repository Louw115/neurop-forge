  "logic": "def matrix_swap_rows(m: dict, row1: int, row2: int) -> dict:\n    \"\"\"Swap two rows.\"\"\"\n    new_rows = [list(r) for r in m[\"rows\"]]\n    new_rows[row1], new_rows[row2] = new_rows[row2], new_rows[row1]\n    return create_matrix(new_rows)",
