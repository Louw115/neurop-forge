  "logic": "def format_coordinate(v0: float, v1: float) -> str:\n    v2 = 'N' if v0 >= 0 else 'S'\n    v3 = 'E' if v1 >= 0 else 'W'\n    return f'{abs(v0):.6f}\u00b0{v2}, {abs(v1):.6f}\u00b0{v3}'",
