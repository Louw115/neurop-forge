  "logic": "def is_deployment_frozen(freeze_periods: list, current_date: str) -> bool:\n    \"\"\"Check if current date is in a freeze period.\"\"\"\n    for period in freeze_periods:\n        if period.get(\"start\") <= current_date <= period.get(\"end\"):\n            return True\n    return False",
