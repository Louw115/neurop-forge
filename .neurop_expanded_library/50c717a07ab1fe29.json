  "logic": "def calculate_compound_tax(amount: float, rates: list) -> dict:\n    \"\"\"Calculate compound taxes (each on previous).\"\"\"\n    current = amount\n    taxes = []\n    for rate in rates:\n        tax = round(current * rate / 100, 2)\n        taxes.append(tax)\n        current += tax\n    return {\n        \"subtotal\": amount,\n        \"taxes\": taxes,\n        \"total\": current\n    }",
