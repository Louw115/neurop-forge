  "logic": "def slugify(v0: str) -> str:\n    if not v0:\n        return ''\n    v1 = []\n    for v2 in v0.lower():\n        if v2.isalnum():\n            v1.append(v2)\n        elif v2 in ' -_':\n            if v1 and v1[-1] != '-':\n                v1.append('-')\n    v3 = ''.join(v1)\n    return v3.strip('-')",
