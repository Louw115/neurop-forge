  "logic": "def get_country_for_code(country_code: str) -> str:\n    \"\"\"Get country name for a country code.\"\"\"\n    codes = {\n        '1': 'United States/Canada',\n        '44': 'United Kingdom',\n        '33': 'France',\n        '49': 'Germany',\n        '39': 'Italy',\n        '34': 'Spain',\n        '81': 'Japan',\n        '86': 'China',\n        '91': 'India',\n        '61': 'Australia',\n        '55': 'Brazil',\n        '52': 'Mexico',\n        '7': 'Russia'\n    }\n    return codes.get(country_code.lstrip('+'), 'Unknown')",
