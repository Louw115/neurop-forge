  "logic": "def to_snakecase(text: str) -> str:\n    \"\"\"Convert to snake_case.\"\"\"\n    text = re.sub(r'([A-Z]+)([A-Z][a-z])', r'\\1_\\2', text)\n    text = re.sub(r'([a-z\\d])([A-Z])', r'\\1_\\2', text)\n    text = re.sub(r'[\\s\\-]+', '_', text)\n    return text.lower()",
