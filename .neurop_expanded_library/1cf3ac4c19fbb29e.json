  "logic": "def interpolate_translation(v0: str, v1: dict) -> str:\n    v2 = v0\n    for v3, v4 in v1.items():\n        v2 = v2.replace(f'{{{v3}}}', str(v4))\n        v2 = v2.replace(f'{{{{ {v3} }}}}', str(v4))\n    return v2",
