  "logic": "def build_cors_headers(v0: str, v1: list, v2: list, v3: int) -> dict:\n    v4 = {}\n    if v0:\n        v4['Access-Control-Allow-Origin'] = v0\n    if v1:\n        v4['Access-Control-Allow-Methods'] = ', '.join(v1)\n    if v2:\n        v4['Access-Control-Allow-Headers'] = ', '.join(v2)\n    if v3 > 0:\n        v4['Access-Control-Max-Age'] = str(v3)\n    return v4",
