  "logic": "def is_balanced_parentheses(v0: str) -> bool:\n    v1 = []\n    v2 = {')': '(', '}': '{', ']': '['}\n    for v3 in v0:\n        if v3 in '({[':\n            v1.append(v3)\n        elif v3 in ')}]':\n            if not v1 or v1[-1] != v2[v3]:\n                return False\n            v1.pop()\n    return len(v1) == 0",
