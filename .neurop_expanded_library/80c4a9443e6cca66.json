  "logic": "def validate_coupon_code(v0: str, v1: dict, v2: str) -> dict:\n    if v0 not in v1:\n        return {'valid': False, 'error': 'Invalid coupon code'}\n    v3 = v1[v0]\n    if v3.get('expires') and v2 > v3['expires']:\n        return {'valid': False, 'error': 'Coupon has expired'}\n    if v3.get('uses_remaining', 1) <= 0:\n        return {'valid': False, 'error': 'Coupon usage limit reached'}\n    return {'valid': True, 'discount': v3.get('discount', 0), 'type': v3.get('type', 'percent')}",
