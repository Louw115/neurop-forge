  "logic": "def validate_password(v0: str, v1: dict) -> dict:\n    v2 = []\n    if v1.get('min_length') and (not v3(v0, v1['min_length'])):\n        v2.append(f\"Must be at least {v1['min_length']} characters\")\n    if v1.get('max_length') and (not v4(v0, v1['max_length'])):\n        v2.append(f\"Must be at most {v1['max_length']} characters\")\n    if v1.get('require_uppercase') and (not v5(v0)):\n        v2.append('Must contain uppercase letter')\n    if v1.get('require_lowercase') and (not v6(v0)):\n        v2.append('Must contain lowercase letter')\n    if v1.get('require_digit') and (not v7(v0)):\n        v2.append('Must contain digit')\n    if v1.get('require_special') and (not v8(v0)):\n        v2.append('Must contain special character')\n    return {'valid': len(v2) == 0, 'errors': v2}",
