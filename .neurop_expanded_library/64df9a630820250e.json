  "logic": "def build_hateoas_links(v0: str, v1: str, v2: str, v3: list) -> list:\n    v4 = [{'rel': 'self', 'href': f'{v2}/{v0}/{v1}', 'method': 'GET'}]\n    for v5 in v3:\n        v6 = v7(v5)\n        if v5 in ('update', 'delete'):\n            v8 = f'{v2}/{v0}/{v1}'\n        else:\n            v8 = f'{v2}/{v0}/{v1}/{v5}'\n        v4.append({'rel': v5, 'href': v8, 'method': v6})\n    return v4",
