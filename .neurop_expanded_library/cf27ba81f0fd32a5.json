  "logic": "def calculate_thumbnail_dimensions(v0: int, v1: int, v2: int) -> dict:\n    if v0 <= v2 and v1 <= v2:\n        return {'width': v0, 'height': v1}\n    if v0 > v1:\n        return {'width': v2, 'height': int(v1 * v2 / v0)}\n    return {'width': int(v0 * v2 / v1), 'height': v2}",
