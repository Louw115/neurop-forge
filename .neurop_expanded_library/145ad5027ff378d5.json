  "logic": "def stack_peek_at(stack: dict, position: int):\n    \"\"\"Peek at item at position from top.\"\"\"\n    if position < 0 or position >= len(stack[\"items\"]):\n        return None\n    return stack[\"items\"][-(position + 1)]",
