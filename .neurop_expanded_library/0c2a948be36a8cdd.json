  "logic": "def validate_file_extension(v0: str, v1: list) -> dict:\n    v2 = v3(v0)\n    if v2 not in [v4.lower() for v4 in v1]:\n        return {'valid': False, 'error': f\"File type .{v2} not allowed. Allowed: {', '.join(v1)}\"}\n    return {'valid': True, 'error': None}",
