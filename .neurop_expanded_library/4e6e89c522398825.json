  "logic": "def to_csv_line(values: list, delimiter: str) -> str:\n    \"\"\"Convert a list of values to a CSV line.\"\"\"\n    if not values:\n        return \"\"\n    if not delimiter:\n        delimiter = ','\n    return delimiter.join(str(v) for v in values)",
