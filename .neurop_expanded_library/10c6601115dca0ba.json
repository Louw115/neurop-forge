  "logic": "def partition_by_boolean(items: list, predicate) -> dict:\n    \"\"\"Partition items by boolean predicate.\"\"\"\n    true_items = []\n    false_items = []\n    for item in items:\n        if predicate(item):\n            true_items.append(item)\n        else:\n            false_items.append(item)\n    return {\"true\": true_items, \"false\": false_items}",
