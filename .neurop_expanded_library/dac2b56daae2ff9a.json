  "logic": "def random_string(seed: int, length: int, charset: str) -> str:\n    \"\"\"Generate deterministic random string.\"\"\"\n    result = []\n    for i in range(length):\n        idx = random_int(hash_seed(seed, f\"char_{i}\"), 0, len(charset) - 1)\n        result.append(charset[idx])\n    return ''.join(result)",
