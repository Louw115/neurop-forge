  "logic": "def assert_in(value, container, message: str) -> dict:\n    \"\"\"Assert value is in container.\"\"\"\n    passed = value in container\n    return {\n        \"passed\": passed,\n        \"message\": message,\n        \"actual\": str(value),\n        \"expected\": f\"in {container}\",\n        \"error\": None if passed else f\"{value} not found in container\"\n    }",
