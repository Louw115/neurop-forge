  "logic": "def parse_cron_expression(expression: str) -> dict:\n    \"\"\"Parse a cron expression.\"\"\"\n    parts = expression.split()\n    if len(parts) != 5:\n        return {\"valid\": False}\n    return {\n        \"valid\": True,\n        \"minute\": parts[0],\n        \"hour\": parts[1],\n        \"day_of_month\": parts[2],\n        \"month\": parts[3],\n        \"day_of_week\": parts[4]\n    }",
