  "logic": "def matrix_is_symmetric(m: dict) -> bool:\n    \"\"\"Check if matrix is symmetric.\"\"\"\n    if not matrix_is_square(m):\n        return False\n    for i in range(m[\"num_rows\"]):\n        for j in range(i + 1, m[\"num_cols\"]):\n            if m[\"rows\"][i][j] != m[\"rows\"][j][i]:\n                return False\n    return True",
