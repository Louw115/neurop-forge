  "logic": "def calculate_bundle_discount(v0: list, v1: float) -> dict:\n    v2 = sum((v3['price'] for v3 in v0))\n    v4 = v5(v2, v1)\n    return {'items': len(v0), 'original': v2, 'bundle_discount': v1, 'savings': round(v2 - v4, 2), 'final': v4}",
