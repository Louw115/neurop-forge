  "logic": "def calculate_flag_stats(v0: dict) -> dict:\n    v1 = len(v0)\n    v2 = sum((1 for v3 in v0.values() if v3.get('enabled')))\n    v4 = sum((1 for v3 in v0.values() if v3.get('rules')))\n    v5 = sum((1 for v3 in v0.values() if v3.get('variants')))\n    return {'total': v1, 'enabled': v2, 'disabled': v1 - v2, 'with_rules': v4, 'with_variants': v5}",
