  "logic": "def format_duration_seconds(v0: int) -> str:\n    if v0 < 0:\n        return '0s'\n    if v0 < 60:\n        return f'{v0}s'\n    v1 = v0 // 60\n    v2 = v0 % 60\n    if v1 < 60:\n        return f'{v1}m {v2}s' if v2 else f'{v1}m'\n    v3 = v1 // 60\n    v4 = v1 % 60\n    if v3 < 24:\n        v5 = [f'{v3}h']\n        if v4:\n            v5.append(f'{v4}m')\n        if v2:\n            v5.append(f'{v2}s')\n        return ' '.join(v5)\n    v6 = v3 // 24\n    v7 = v3 % 24\n    v5 = [f'{v6}d']\n    if v7:\n        v5.append(f'{v7}h')\n    if v4:\n        v5.append(f'{v4}m')\n    return ' '.join(v5)",
