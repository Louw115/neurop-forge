  "logic": "def validate_hostname(v0: str) -> bool:\n    import re\n    if len(v0) > 253:\n        return False\n    v1 = '^([a-zA-Z0-9]([a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?\\\\.)*[a-zA-Z]{2,}$'\n    return bool(v2.match(v1, v0))",
