  "logic": "def parse_dsn(v0: str) -> dict:\n    v1 = v2(v0)\n    if 'auth' in v1 and ':' in v1['auth']:\n        v3, v4 = v1['auth'].split(':', 1)\n        v1['user'] = v3\n        v1['password'] = v4\n    if 'path' in v1 and '?' in v1['path']:\n        v5, v6 = v1['path'].split('?', 1)\n        v1['path'] = v5\n        v1['database'] = v5\n        v7 = {}\n        for v8 in v6.split('&'):\n            if '=' in v8:\n                v9, v10 = v8.split('=', 1)\n                v7[v9] = v10\n        v1['params'] = v7\n    return v1",
