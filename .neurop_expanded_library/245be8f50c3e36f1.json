  "logic": "def check_in_list(v0, v1: list, v2: str) -> dict:\n    if v0 not in v1:\n        return {'valid': False, 'error': f\"{v2} must be one of: {', '.join((str(v3) for v3 in v1))}\"}\n    return {'valid': True, 'error': None}",
