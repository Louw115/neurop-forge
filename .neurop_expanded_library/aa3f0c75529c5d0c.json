  "logic": "def list_to_string(items: list, separator: str) -> str:\n    \"\"\"Convert a list to a string using a separator.\"\"\"\n    if not items:\n        return \"\"\n    if not separator:\n        separator = ','\n    return separator.join(str(item) for item in items)",
