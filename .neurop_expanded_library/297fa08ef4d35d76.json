  "logic": "def calculate_irr_approx(cash_flows: list, max_iterations: int, tolerance: float) -> float:\n    \"\"\"Calculate approximate internal rate of return.\"\"\"\n    if not cash_flows:\n        return 0.0\n    low_rate, high_rate = -50.0, 100.0\n    for _ in range(max_iterations):\n        mid_rate = (low_rate + high_rate) / 2\n        npv = calculate_npv(cash_flows, mid_rate)\n        if abs(npv) < tolerance:\n            return mid_rate\n        if npv > 0:\n            low_rate = mid_rate\n        else:\n            high_rate = mid_rate\n    return (low_rate + high_rate) / 2",
