  "logic": "def detect_command_injection(input_str: str) -> bool:\n    \"\"\"Detect command injection patterns.\"\"\"\n    patterns = [\";\", \"|\", \"&\", \"`\", \"$(\", \"${\", \"\\n\", \"\\r\"]\n    return any(pattern in input_str for pattern in patterns)",
