  "logic": "def calculate_color_distance(color1: dict, color2: dict) -> float:\n    \"\"\"Calculate Euclidean distance between colors.\"\"\"\n    dr = color1[\"r\"] - color2[\"r\"]\n    dg = color1[\"g\"] - color2[\"g\"]\n    db = color1[\"b\"] - color2[\"b\"]\n    return (dr**2 + dg**2 + db**2) ** 0.5",
