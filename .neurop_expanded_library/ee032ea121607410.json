  "logic": "def build_limit_offset(limit: int, offset: int) -> str:\n    \"\"\"Build a LIMIT ... OFFSET clause.\"\"\"\n    parts = []\n    if limit > 0:\n        parts.append(f\"LIMIT {limit}\")\n    if offset > 0:\n        parts.append(f\"OFFSET {offset}\")\n    return \" \".join(parts)",
