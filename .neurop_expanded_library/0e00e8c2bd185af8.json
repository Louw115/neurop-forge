  "logic": "def calculate_manhattan_distance(vec1: list, vec2: list) -> float:\n    \"\"\"Calculate Manhattan distance between two vectors.\"\"\"\n    if len(vec1) != len(vec2):\n        return 0.0\n    return sum(abs(a - b) for a, b in zip(vec1, vec2))",
