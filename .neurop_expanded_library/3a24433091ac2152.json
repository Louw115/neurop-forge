  "logic": "def delete_nested(obj: dict, path: str) -> dict:\n    \"\"\"Delete nested key, returning new object.\"\"\"\n    keys = path.split(\".\")\n    if len(keys) == 1:\n        return delete_key(obj, keys[0])\n    result = dict(obj)\n    current = result\n    for key in keys[:-1]:\n        if key not in current:\n            return result\n        current[key] = dict(current[key])\n        current = current[key]\n    if keys[-1] in current:\n        del current[keys[-1]]\n    return result",
