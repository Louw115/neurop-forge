  "logic": "def apply_tiered_discount(v0: float, v1: list) -> dict:\n    for v2 in sorted(v1, key=lambda v3: -v3['threshold']):\n        if v0 >= v2['threshold']:\n            v4 = v2.get('percentage', 0)\n            v5 = v6(v0, v4)\n            return {'original': v0, 'discount_percentage': v4, 'discount_amount': round(v0 - v5, 2), 'final': v5}\n    return {'original': v0, 'discount_percentage': 0, 'discount_amount': 0, 'final': v0}",
