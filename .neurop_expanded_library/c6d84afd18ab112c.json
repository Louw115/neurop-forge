  "logic": "def adjust_hue(r: int, g: int, b: int, degrees: int) -> dict:\n    \"\"\"Adjust hue by degrees.\"\"\"\n    hsl = rgb_to_hsl(r, g, b)\n    new_h = (hsl[\"h\"] + degrees) % 360\n    return hsl_to_rgb(new_h, hsl[\"s\"], hsl[\"l\"])",
