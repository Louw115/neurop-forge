  "logic": "def bst_to_dict(tree: dict) -> dict:\n    \"\"\"Convert BST to dictionary.\"\"\"\n    result = {}\n    def collect(node):\n        if node:\n            collect(node[\"left\"])\n            result[node[\"key\"]] = node[\"value\"]\n            collect(node[\"right\"])\n    collect(tree[\"root\"])\n    return result",
