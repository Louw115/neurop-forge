  "logic": "def calculate_latency_percentiles(v0: list) -> dict:\n    if not v0:\n        return {'p50': 0, 'p90': 0, 'p95': 0, 'p99': 0}\n    v1 = sorted(v0)\n    v2 = len(v1)\n    return {'p50': v1[int(v2 * 0.5)], 'p90': v1[int(v2 * 0.9)], 'p95': v1[int(v2 * 0.95)], 'p99': v1[min(int(v2 * 0.99), v2 - 1)]}",
