  "logic": "def parse_cache_control(header: str) -> dict:\n    \"\"\"Parse a Cache-Control header into directives.\"\"\"\n    if not header:\n        return {}\n    result = {}\n    parts = header.split(',')\n    for part in parts:\n        part = part.strip()\n        if '=' in part:\n            key, value = part.split('=', 1)\n            try:\n                result[key.strip()] = int(value.strip())\n            except ValueError:\n                result[key.strip()] = value.strip()\n        else:\n            result[part] = True\n    return result",
