  "logic": "def export_contacts_csv_row(contact: dict) -> str:\n    \"\"\"Export contact as CSV row.\"\"\"\n    name = contact.get(\"name\", \"\").replace('\"', '\"\"')\n    email = contact.get(\"email\", \"\").replace('\"', '\"\"')\n    phone = contact.get(\"phone\", \"\").replace('\"', '\"\"')\n    return f'\"{name}\",\"{email}\",\"{phone}\"'",
