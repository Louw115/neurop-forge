  "logic": "def extract_text_from_html(html: str) -> str:\n    \"\"\"Extract plain text from HTML.\"\"\"\n    import re\n    text = re.sub(r'<script[^>]*>.*?</script>', '', html, flags=re.DOTALL)\n    text = re.sub(r'<style[^>]*>.*?</style>', '', text, flags=re.DOTALL)\n    text = re.sub(r'<[^>]+>', ' ', text)\n    text = re.sub(r'\\s+', ' ', text)\n    return text.strip()",
