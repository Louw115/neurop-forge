  "logic": "def is_parallel_step_complete(v0: dict) -> bool:\n    v1 = v0.get('branch_statuses', {})\n    v2 = v0.get('join_type', 'all')\n    if v2 == 'all':\n        return v3((v4 == 'completed' for v4 in v1.values()))\n    elif v2 == 'any':\n        return v5((v4 == 'completed' for v4 in v1.values()))\n    return False",
