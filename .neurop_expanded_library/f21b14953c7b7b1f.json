  "logic": "def calculate_sales_tax(v0: float, v1: float, v2: float) -> dict:\n    v3 = round(v0 * v1 / 100, 2)\n    v4 = round(v0 * v2 / 100, 2)\n    return {'subtotal': v0, 'state_tax': v3, 'local_tax': v4, 'total_tax': round(v3 + v4, 2), 'total': round(v0 + v3 + v4, 2)}",
