  "logic": "def validate_array_length(v0: list, v1: int, v2: int, v3: str) -> dict:\n    if len(v0) < v1:\n        return {'valid': False, 'error': f'{v3} must have at least {v1} items'}\n    if v2 > 0 and len(v0) > v2:\n        return {'valid': False, 'error': f'{v3} must have at most {v2} items'}\n    return {'valid': True, 'error': None}",
