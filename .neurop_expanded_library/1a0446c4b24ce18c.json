  "logic": "def insert_bits(value: int, bits: int, start: int, count: int) -> int:\n    \"\"\"Insert bits at position.\"\"\"\n    mask = (1 << count) - 1\n    cleared = value & ~(mask << start)\n    return cleared | ((bits & mask) << start)",
