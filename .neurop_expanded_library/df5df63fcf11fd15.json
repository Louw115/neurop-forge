  "logic": "def wrap_text(v0: str, v1: int) -> list:\n    v2 = v0.split()\n    v3 = []\n    v4 = []\n    v5 = 0\n    for v6 in v2:\n        if v5 + len(v6) + len(v4) <= v1:\n            v4.append(v6)\n            v5 += len(v6)\n        else:\n            if v4:\n                v3.append(' '.join(v4))\n            v4 = [v6]\n            v5 = len(v6)\n    if v4:\n        v3.append(' '.join(v4))\n    return v3",
