  "logic": "def cancel_workflow(v0: dict, v1: str, v2: str) -> dict:\n    v3 = dict(v0)\n    v3['status'] = 'cancelled'\n    v3['cancelled_at'] = v1\n    v3['cancellation_reason'] = v2\n    v3['history'] = list(v0.get('history', []))\n    v3['history'].append({'action': 'cancelled', 'reason': v2, 'timestamp': v1})\n    return v3",
