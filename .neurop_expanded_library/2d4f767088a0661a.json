  "logic": "def bloom_from_bytes(data: bytes, size: int, num_hashes: int) -> dict:\n    \"\"\"Deserialize bloom filter from bytes.\"\"\"\n    bits = [0] * size\n    for i in range(min(size, len(data) * 8)):\n        if data[i // 8] & (1 << (i % 8)):\n            bits[i] = 1\n    return {\n        \"bits\": bits,\n        \"size\": size,\n        \"num_hashes\": num_hashes,\n        \"count\": 0\n    }",
