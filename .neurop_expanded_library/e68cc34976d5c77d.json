  "logic": "def build_cache_control(v0: int, v1: bool, v2: bool, v3: bool, v4: bool) -> str:\n    v5 = []\n    if v2:\n        v5.append('no-store')\n    elif v3:\n        v5.append('no-cache')\n    else:\n        if v1:\n            v5.append('private')\n        else:\n            v5.append('public')\n        if v0 >= 0:\n            v5.append(f'max-age={v0}')\n    if v4:\n        v5.append('must-revalidate')\n    return ', '.join(v5)",
