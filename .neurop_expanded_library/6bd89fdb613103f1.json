  "logic": "def remove_comments(query: str) -> str:\n    \"\"\"Remove SQL comments from a query.\"\"\"\n    import re\n    query = re.sub(r'--[^\\n]*', '', query)\n    query = re.sub(r'/\\*.*?\\*/', '', query, flags=re.DOTALL)\n    return query.strip()",
