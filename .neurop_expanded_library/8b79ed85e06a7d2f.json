  "logic": "def bloom_to_bytes(bloom: dict) -> bytes:\n    \"\"\"Serialize bloom filter to bytes.\"\"\"\n    byte_count = (bloom[\"size\"] + 7) // 8\n    result = bytearray(byte_count)\n    for i, bit in enumerate(bloom[\"bits\"]):\n        if bit:\n            result[i // 8] |= (1 << (i % 8))\n    return bytes(result)",
