  "logic": "def create_list(items: list, ordered: bool) -> str:\n    \"\"\"Create HTML list.\"\"\"\n    tag = \"ol\" if ordered else \"ul\"\n    list_items = \"\".join(f\"<li>{escape_html(str(item))}</li>\" for item in items)\n    return f\"<{tag}>{list_items}</{tag}>\"",
