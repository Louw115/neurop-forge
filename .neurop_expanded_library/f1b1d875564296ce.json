  "logic": "def interval_to_range(interval: dict, step: float) -> list:\n    \"\"\"Generate range of values in interval.\"\"\"\n    result = []\n    current = interval[\"start\"]\n    while current <= interval[\"end\"]:\n        result.append(current)\n        current += step\n    return result",
