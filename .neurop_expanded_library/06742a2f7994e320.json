  "logic": "def assert_match(text: str, pattern: str) -> dict:\n    \"\"\"Assert text matches pattern.\"\"\"\n    import re\n    passed = bool(re.match(pattern, text))\n    return {\n        \"passed\": passed,\n        \"text\": text,\n        \"pattern\": pattern,\n        \"message\": \"\" if passed else f\"Expected to match {pattern}\"\n    }",
