  "logic": "def calculate_notification_stats(v0: list) -> dict:\n    v1 = len(v0)\n    v2 = sum((1 for v3 in v0 if v3.get('sent_at')))\n    v4 = sum((1 for v3 in v0 if v3.get('read')))\n    v5 = {}\n    v6 = {}\n    for v3 in v0:\n        v7 = v3.get('channel', 'unknown')\n        v8 = v3.get('type', 'unknown')\n        v5[v7] = v5.get(v7, 0) + 1\n        v6[v8] = v6.get(v8, 0) + 1\n    return {'total': v1, 'sent': v2, 'read': v4, 'read_rate': v4 / v2 * 100 if v2 > 0 else 0, 'by_channel': v5, 'by_type': v6}",
