  "logic": "def extract_front_matter(v0: str) -> dict:\n    if not v0.startswith('---'):\n        return {'frontmatter': {}, 'content': v0}\n    v1 = v0.split('---', 2)\n    if len(v1) < 3:\n        return {'frontmatter': {}, 'content': v0}\n    v2 = {}\n    for v3 in v1[1].strip().split('\\n'):\n        if ':' in v3:\n            v4, v5 = v3.split(':', 1)\n            v2[v4.strip()] = v5.strip()\n    return {'frontmatter': v2, 'content': v1[2].strip()}",
