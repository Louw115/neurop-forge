  "logic": "def format_xml_element(v0: str, v1, v2: dict) -> str:\n    v3 = ' '.join((f'{v4}=\"{v5}\"' for v4, v5 in v2.items()))\n    if v3:\n        v3 = ' ' + v3\n    if isinstance(v1, dict):\n        v6 = ''.join((v7(v4, v5, {}) for v4, v5 in v1.items()))\n        return f'<{v0}{v3}>{v6}</{v0}>'\n    elif isinstance(v1, list):\n        v6 = ''.join((v7('item', v5, {}) for v5 in v1))\n        return f'<{v0}{v3}>{v6}</{v0}>'\n    else:\n        v8 = str(v1).replace('&', '&amp;').replace('<', '&lt;').replace('>', '&gt;')\n        return f'<{v0}{v3}>{v8}</{v0}>'",
