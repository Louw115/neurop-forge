  "logic": "def format_compact_currency(v0: float, v1: str) -> str:\n    if abs(v0) >= 1000000000:\n        return f'{v1}{v0 / 1000000000:.1f}B'\n    if abs(v0) >= 1000000:\n        return f'{v1}{v0 / 1000000:.1f}M'\n    if abs(v0) >= 1000:\n        return f'{v1}{v0 / 1000:.1f}K'\n    return f'{v1}{v0:.2f}'",
