  "logic": "def get_ancestors(root: dict, node_id: str) -> list:\n    \"\"\"Get all ancestor nodes.\"\"\"\n    path = get_node_path(root, node_id)\n    if len(path) <= 1:\n        return []\n    ancestors = []\n    current = root\n    for p in path[:-1]:\n        if current[\"id\"] == p:\n            ancestors.append(current)\n        for child in current.get(\"children\", []):\n            if child[\"id\"] in path:\n                current = child\n                break\n    return ancestors",
