  "logic": "def create_anchor(text: str) -> str:\n    \"\"\"Create a heading anchor from text.\"\"\"\n    anchor = text.lower()\n    anchor = re.sub(r'[^\\w\\s-]', '', anchor)\n    anchor = re.sub(r'[\\s]+', '-', anchor)\n    return anchor",
