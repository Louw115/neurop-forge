  "logic": "def calculate_rate_limit_window(requests: int, window_seconds: int, current_count: int) -> dict:\n    \"\"\"Calculate rate limit status.\"\"\"\n    remaining = max(0, requests - current_count)\n    return {\n        \"limit\": requests,\n        \"remaining\": remaining,\n        \"reset_after_seconds\": window_seconds,\n        \"exceeded\": remaining == 0\n    }",
