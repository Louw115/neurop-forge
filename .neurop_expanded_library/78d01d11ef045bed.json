  "logic": "def get_severity(v0: str) -> str:\n    v1 = v2(v0)\n    if v1 in ['SERVER', 'TIMEOUT']:\n        return 'error'\n    if v1 in ['AUTH', 'PERM']:\n        return 'warning'\n    return 'info'",
