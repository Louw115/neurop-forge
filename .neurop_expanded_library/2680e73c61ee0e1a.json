  "logic": "def open_tag(v0: dict, v1: str, v2: dict) -> dict:\n    v3 = ''\n    for v4, v5 in v2.items():\n        v3 += f' {v4}=\"{v5}\"'\n    v6 = '  ' * v0.get('indent', 0)\n    v7 = f'{v6}<{v1}{v3}>\\n'\n    return {'parts': v0['parts'] + [v7], 'length': v0['length'] + len(v7), 'indent': v0.get('indent', 0) + 1}",
