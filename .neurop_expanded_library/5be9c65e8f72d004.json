  "logic": "def build_join_clause(v0: str, v1: str, v2: str, v3: str) -> str:\n    v4 = ('INNER', 'LEFT', 'RIGHT', 'FULL', 'CROSS')\n    v5 = v0.upper() if v0.upper() in v4 else 'INNER'\n    return f'{v5} JOIN {v6(v1)} ON {v6(v2)} = {v6(v3)}'",
