  "logic": "def calculate_rollout_percentage(current_step: int, total_steps: int) -> float:\n    \"\"\"Calculate gradual rollout percentage.\"\"\"\n    if total_steps <= 0:\n        return 100.0\n    return min(100.0, (current_step / total_steps) * 100)",
