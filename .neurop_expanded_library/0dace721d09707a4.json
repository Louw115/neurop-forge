  "logic": "def add_text(builder: dict, text: str) -> dict:\n    \"\"\"Add text content.\"\"\"\n    indent = \"  \" * builder.get(\"indent\", 0)\n    full_text = f\"{indent}{text}\\n\"\n    return {\n        \"parts\": builder[\"parts\"] + [full_text],\n        \"length\": builder[\"length\"] + len(full_text),\n        \"indent\": builder.get(\"indent\", 0)\n    }",
