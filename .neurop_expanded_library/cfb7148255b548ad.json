  "logic": "def wrap_in_count(query: str) -> str:\n    \"\"\"Wrap a SELECT query in COUNT(*) to get row count.\"\"\"\n    cleaned = query.rstrip().rstrip(';')\n    return f\"SELECT COUNT(*) FROM ({cleaned}) AS count_subquery\"",
