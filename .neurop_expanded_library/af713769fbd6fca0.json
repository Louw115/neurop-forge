  "logic": "def validate_filter_operators(v0: dict, v1: list) -> dict:\n    v2 = []\n    for v3 in v0.keys():\n        if '__' in v3:\n            _, v4 = v3.rsplit('__', 1)\n            if v4 not in v1:\n                v2.append(f\"Operator '{v4}' not allowed\")\n    return {'valid': len(v2) == 0, 'errors': v2}",
