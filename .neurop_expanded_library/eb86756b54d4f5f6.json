  "logic": "def format_progress_bar(progress: float, width: int, filled_char: str, empty_char: str) -> str:\n    \"\"\"Format a text-based progress bar.\"\"\"\n    filled = int(progress / 100 * width)\n    empty = width - filled\n    return filled_char * filled + empty_char * empty",
