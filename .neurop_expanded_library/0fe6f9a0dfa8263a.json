  "logic": "def parse_json_path(path: str) -> list:\n    \"\"\"Parse a JSON path into parts.\"\"\"\n    import re\n    if not path.startswith(\"$\"):\n        return []\n    parts = []\n    for match in re.finditer(r'\\.([a-zA-Z_][a-zA-Z0-9_]*)|\\[(\\d+)\\]', path):\n        if match.group(1):\n            parts.append(match.group(1))\n        else:\n            parts.append(int(match.group(2)))\n    return parts",
