  "logic": "def group_emails_by_domain(emails: list) -> dict:\n    \"\"\"Group emails by their domain.\"\"\"\n    groups = {}\n    for email in emails:\n        domain = extract_domain(email)\n        if domain not in groups:\n            groups[domain] = []\n        groups[domain].append(email)\n    return groups",
