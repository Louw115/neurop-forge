  "logic": "def extract_sort_params(v0: str) -> list:\n    if not v0:\n        return []\n    v1 = []\n    for v2 in v0.split(','):\n        v2 = v2.strip()\n        if v2.startswith('-'):\n            v1.append({'field': v2[1:], 'direction': 'desc'})\n        elif v2.startswith('+'):\n            v1.append({'field': v2[1:], 'direction': 'asc'})\n        else:\n            v1.append({'field': v2, 'direction': 'asc'})\n    return v1",
