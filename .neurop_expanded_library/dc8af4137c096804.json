  "logic": "def get_pem_type(pem: str) -> str:\n    \"\"\"Extract the type from a PEM header.\"\"\"\n    if not pem:\n        return \"\"\n    first_line = pem.strip().split('\\n')[0]\n    if first_line.startswith('-----BEGIN ') and first_line.endswith('-----'):\n        return first_line[11:-5]\n    return \"\"",
