  "logic": "def create_table(v0: list, v1: list) -> str:\n    v2 = '<thead><tr>' + ''.join((f'<th>{v3(str(v4))}</th>' for v4 in v0)) + '</tr></thead>'\n    v5 = ''\n    for v6 in v1:\n        v5 += '<tr>' + ''.join((f'<td>{v3(str(v7))}</td>' for v7 in v6)) + '</tr>'\n    v8 = f'<tbody>{v5}</tbody>'\n    return f'<table>{v2}{v8}</table>'",
