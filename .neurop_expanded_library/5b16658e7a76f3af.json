  "logic": "def trie_search(trie: dict, key: str) -> dict:\n    \"\"\"Search for a key in trie.\"\"\"\n    node = trie[\"root\"]\n    for char in key:\n        if char not in node[\"children\"]:\n            return {\"found\": False, \"value\": None}\n        node = node[\"children\"][char]\n    if node[\"is_end\"]:\n        return {\"found\": True, \"value\": node[\"value\"]}\n    return {\"found\": False, \"value\": None}",
