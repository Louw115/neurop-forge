  "logic": "def parse_connection_string(v0: str) -> dict:\n    v1 = {'raw': v0}\n    if '://' in v0:\n        v2, v3 = v0.split('://', 1)\n        v1['protocol'] = v2\n        if '@' in v3:\n            v4, v5 = v3.rsplit('@', 1)\n            v1['auth'] = v4\n            v3 = v5\n        if '/' in v3:\n            v6, v7 = v3.split('/', 1)\n            v1['path'] = v7\n        else:\n            v6 = v3\n        if ':' in v6:\n            v8, v9 = v6.rsplit(':', 1)\n            v1['host'] = v8\n            v1['port'] = v9\n        else:\n            v1['host'] = v6\n    return v1",
