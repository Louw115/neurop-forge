  "logic": "def validate_model_schema(v0: dict, v1: list, v2: dict) -> dict:\n    v3 = []\n    for v4 in v1:\n        if v4 not in v0 or v0[v4] is None:\n            v3.append(f'Missing required field: {v4}')\n    for v4, v5 in v2.items():\n        if v4 in v0 and v0[v4] is not None:\n            if v5 == 'string' and (not isinstance(v0[v4], str)):\n                v3.append(f'{v4} must be a string')\n            elif v5 == 'integer' and (not isinstance(v0[v4], int)):\n                v3.append(f'{v4} must be an integer')\n            elif v5 == 'number' and (not isinstance(v0[v4], (int, float))):\n                v3.append(f'{v4} must be a number')\n            elif v5 == 'boolean' and (not isinstance(v0[v4], bool)):\n                v3.append(f'{v4} must be a boolean')\n    return {'valid': len(v3) == 0, 'errors': v3}",
