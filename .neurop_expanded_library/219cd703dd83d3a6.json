  "logic": "def dot_product(vec1: list, vec2: list) -> float:\n    \"\"\"Calculate dot product of two vectors.\"\"\"\n    if len(vec1) != len(vec2):\n        return 0.0\n    return sum(a * b for a, b in zip(vec1, vec2))",
