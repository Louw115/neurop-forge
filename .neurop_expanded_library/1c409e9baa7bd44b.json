  "logic": "def estimate_quota_depletion(current_remaining: int, rate_per_second: float, current_time: float) -> float:\n    \"\"\"Estimate when quota will be depleted.\"\"\"\n    if rate_per_second <= 0:\n        return float(\"inf\")\n    return current_time + (current_remaining / rate_per_second)",
