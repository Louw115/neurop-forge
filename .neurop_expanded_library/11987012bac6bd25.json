  "logic": "def has_special_char(password: str) -> bool:\n    \"\"\"Check if password has special character.\"\"\"\n    special = set(\"!@#$%^&*()_+-=[]{}|;':\\\",./<>?`~\")\n    return any(c in special for c in password)",
