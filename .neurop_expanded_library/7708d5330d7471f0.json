  "logic": "def has_lowercase(value: str) -> dict:\n    \"\"\"Check if value has lowercase letter.\"\"\"\n    valid = any(c.islower() for c in value)\n    return {\"valid\": valid, \"error\": \"Must contain lowercase letter\" if not valid else None}",
