  "logic": "def add_element(ds: dict, element) -> dict:\n    \"\"\"Add new element as singleton.\"\"\"\n    if element in ds[\"parent\"]:\n        return ds\n    return {\n        \"parent\": {**ds[\"parent\"], element: element},\n        \"rank\": {**ds[\"rank\"], element: 0},\n        \"size\": {**ds[\"size\"], element: 1},\n        \"count\": ds[\"count\"] + 1\n    }",
