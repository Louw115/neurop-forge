  "logic": "def add_months(v0: int, v1: int, v2: int, v3: int) -> dict:\n    v4 = v1 + v3 - 1\n    v5 = v0 + v4 // 12\n    v6 = v4 % 12 + 1\n    v7 = min(v2, v8(v5, v6))\n    return {'year': v5, 'month': v6, 'day': v7}",
