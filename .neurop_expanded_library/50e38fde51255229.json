  "logic": "def apply_quantity_discount(v0: int, v1: float, v2: list) -> dict:\n    for v3 in sorted(v2, key=lambda v4: -v4['min_quantity']):\n        if v0 >= v3['min_quantity']:\n            v5 = v3.get('percentage', 0)\n            v6 = v0 * v1\n            v7 = v8(v6, v5)\n            return {'quantity': v0, 'unit_price': v1, 'original': v6, 'discount_percentage': v5, 'final': v7}\n    return {'quantity': v0, 'unit_price': v1, 'original': round(v0 * v1, 2), 'discount_percentage': 0, 'final': round(v0 * v1, 2)}",
