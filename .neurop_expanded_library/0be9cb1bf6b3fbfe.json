  "logic": "def format_test_summary(v0: dict) -> str:\n    v1 = [f\"Tests: {v0['total']}\", f\"Passed: {v0['passed']}\", f\"Failed: {v0['failed']}\", f\"Pass Rate: {v0['pass_rate']:.1f}%\"]\n    if v0['failures']:\n        v1.append('\\nFailures:')\n        for v2 in v0['failures']:\n            v1.append(f'  - {v3(v2)}')\n    return '\\n'.join(v1)",
