  "logic": "def validate_uuid_format(v0: str) -> dict:\n    import re\n    v1 = '^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$'\n    if not v2.match(v1, v0.lower()):\n        return {'valid': False, 'error': 'Invalid UUID format'}\n    return {'valid': True, 'error': None}",
