  "logic": "def is_valid_uuid_v4(text: str) -> bool:\n    \"\"\"Check if a string is a valid UUID version 4.\"\"\"\n    pattern = r'^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-4[0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$'\n    return bool(re.match(pattern, text))",
