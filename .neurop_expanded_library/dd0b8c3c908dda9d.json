  "logic": "def check_non_negative(value: float, field_name: str) -> dict:\n    \"\"\"Check if a numeric value is non-negative.\"\"\"\n    if value < 0:\n        return {\"valid\": False, \"error\": f\"{field_name} cannot be negative\"}\n    return {\"valid\": True, \"error\": None}",
