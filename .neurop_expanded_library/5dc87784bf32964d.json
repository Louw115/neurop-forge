  "logic": "def calculate_member_discount(amount: float, tier: str, tiers: dict) -> dict:\n    \"\"\"Calculate member tier discount.\"\"\"\n    rate = tiers.get(tier, 0)\n    final = apply_percentage_discount(amount, rate)\n    return {\n        \"tier\": tier,\n        \"discount_rate\": rate,\n        \"original\": amount,\n        \"savings\": round(amount - final, 2),\n        \"final\": final\n    }",
