  "logic": "def format_fiscal_year(year: int, month: int, fiscal_start: int) -> str:\n    \"\"\"Format fiscal year.\"\"\"\n    if month >= fiscal_start:\n        return f\"FY{year + 1}\"\n    return f\"FY{year}\"",
