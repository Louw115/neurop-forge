  "logic": "def pluralize(v0: str) -> str:\n    if v0.endswith('y') and len(v0) > 1 and (v0[-2] not in 'aeiou'):\n        return v0[:-1] + 'ies'\n    if v0.endswith(('s', 'x', 'z', 'ch', 'sh')):\n        return v0 + 'es'\n    if v0.endswith('f'):\n        return v0[:-1] + 'ves'\n    if v0.endswith('fe'):\n        return v0[:-2] + 'ves'\n    return v0 + 's'",
