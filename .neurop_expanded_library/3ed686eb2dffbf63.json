  "logic": "def unflatten_json(data: dict, separator: str) -> dict:\n    \"\"\"Unflatten JSON back to nested structure.\"\"\"\n    result = {}\n    for key, value in data.items():\n        keys = key.split(separator)\n        current = result\n        for k in keys[:-1]:\n            if k not in current:\n                current[k] = {}\n            current = current[k]\n        current[keys[-1]] = value\n    return result",
