  "logic": "def vector_lerp(v1: dict, v2: dict, t: float) -> dict:\n    \"\"\"Linear interpolation between two vectors.\"\"\"\n    values = [a + (b - a) * t for a, b in zip(v1[\"values\"], v2[\"values\"])]\n    return create_vector(values)",
