  "logic": "def build_count_query(table: str, column: str) -> str:\n    \"\"\"Build a COUNT query.\"\"\"\n    if column == \"*\":\n        return f\"SELECT COUNT(*) FROM {escape_sql_identifier(table)}\"\n    return f\"SELECT COUNT({escape_sql_identifier(column)}) FROM {escape_sql_identifier(table)}\"",
