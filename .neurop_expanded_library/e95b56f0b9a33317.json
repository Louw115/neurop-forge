  "logic": "def escape_markdown(text: str) -> str:\n    \"\"\"Escape markdown special characters.\"\"\"\n    special_chars = ['\\\\', '`', '*', '_', '{', '}', '[', ']', '(', ')', '#', '+', '-', '.', '!', '|']\n    result = text\n    for char in special_chars:\n        result = result.replace(char, '\\\\' + char)\n    return result",
