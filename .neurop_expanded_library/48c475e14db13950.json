  "logic": "def get_sqlite_type(python_type: str) -> str:\n    \"\"\"Map a Python type name to SQLite type.\"\"\"\n    type_map = {\n        \"str\": \"TEXT\",\n        \"int\": \"INTEGER\",\n        \"float\": \"REAL\",\n        \"bool\": \"INTEGER\",\n        \"bytes\": \"BLOB\",\n        \"datetime\": \"TEXT\",\n        \"date\": \"TEXT\",\n        \"time\": \"TEXT\",\n        \"Decimal\": \"TEXT\",\n        \"dict\": \"TEXT\",\n        \"list\": \"TEXT\",\n        \"uuid\": \"TEXT\",\n    }\n    return type_map.get(python_type, \"TEXT\")",
