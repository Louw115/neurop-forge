  "logic": "def evaluate_tree(tree: dict, variables: dict) -> float:\n    \"\"\"Evaluate expression tree.\"\"\"\n    if tree[\"type\"] == \"value\":\n        token = tree[\"token\"]\n        if token[\"type\"] == \"number\":\n            return token[\"value\"]\n        return variables.get(token[\"value\"], 0)\n    left = evaluate_tree(tree[\"left\"], variables)\n    right = evaluate_tree(tree[\"right\"], variables)\n    op = tree[\"operator\"]\n    if op == \"+\":\n        return left + right\n    elif op == \"-\":\n        return left - right\n    elif op == \"*\":\n        return left * right\n    elif op == \"/\":\n        return left / right if right != 0 else 0\n    elif op == \"^\":\n        return left ** right\n    return 0",
