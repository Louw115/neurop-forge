  "logic": "def count_logs_by_level(entries: list) -> dict:\n    \"\"\"Count log entries by level.\"\"\"\n    counts = {}\n    for entry in entries:\n        level = entry.get(\"level\", \"INFO\")\n        counts[level] = counts.get(level, 0) + 1\n    return counts",
