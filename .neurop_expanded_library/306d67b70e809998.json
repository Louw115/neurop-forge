  "logic": "def truncate_stack_trace(v0: str, v1: int) -> str:\n    v2 = v0.split('\\n')\n    if len(v2) <= v1:\n        return v0\n    return '\\n'.join(v2[:v1]) + f'\\n... ({len(v2) - v1} more lines)'",
