  "logic": "def build_collection_response(items: list, total: int, page: int, per_page: int) -> dict:\n    \"\"\"Build a paginated collection response.\"\"\"\n    return {\n        \"success\": True,\n        \"data\": items,\n        \"meta\": {\n            \"total\": total,\n            \"page\": page,\n            \"per_page\": per_page,\n            \"total_pages\": (total + per_page - 1) // per_page if per_page > 0 else 0\n        }\n    }",
