  "logic": "def remove_prefix(slug: str, prefix: str, separator: str) -> str:\n    \"\"\"Remove prefix from slug.\"\"\"\n    full_prefix = f\"{prefix}{separator}\"\n    if slug.startswith(full_prefix):\n        return slug[len(full_prefix):]\n    return slug",
