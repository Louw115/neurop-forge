  "logic": "def parse_user_agent(v0: str) -> dict:\n    import re\n    v1 = {'browser': '', 'version': '', 'os': '', 'device': 'desktop'}\n    if 'Mobile' in v0:\n        v1['device'] = 'mobile'\n    elif 'Tablet' in v0:\n        v1['device'] = 'tablet'\n    v2 = [('Chrome', 'Chrome/(\\\\d+)'), ('Firefox', 'Firefox/(\\\\d+)'), ('Safari', 'Version/(\\\\d+).*Safari'), ('Edge', 'Edg/(\\\\d+)'), ('MSIE', 'MSIE (\\\\d+)')]\n    for v3, v4 in v2:\n        v5 = v6.search(v4, v0)\n        if v5:\n            v1['browser'] = v3\n            v1['version'] = v5.group(1)\n            break\n    if 'Windows' in v0:\n        v1['os'] = 'Windows'\n    elif 'Mac' in v0:\n        v1['os'] = 'macOS'\n    elif 'Linux' in v0:\n        v1['os'] = 'Linux'\n    elif 'Android' in v0:\n        v1['os'] = 'Android'\n    elif 'iOS' in v0 or 'iPhone' in v0:\n        v1['os'] = 'iOS'\n    return v1",
