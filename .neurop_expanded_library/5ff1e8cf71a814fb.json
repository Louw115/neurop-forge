  "logic": "def get_exchange_code(phone: str) -> str:\n    \"\"\"Extract exchange code from US phone number.\"\"\"\n    digits = normalize_phone(phone)\n    if len(digits) == 11 and digits[0] == '1':\n        return digits[4:7]\n    if len(digits) >= 10:\n        return digits[3:6]\n    return \"\"",
