  "logic": "def is_same_origin(url1: str, url2: str) -> bool:\n    \"\"\"Check if two URLs have the same origin (scheme, host, port).\"\"\"\n    p1 = parse_url(url1)\n    p2 = parse_url(url2)\n    return (\n        p1['scheme'].lower() == p2['scheme'].lower() and\n        p1['host'].lower() == p2['host'].lower() and\n        p1['port'] == p2['port']\n    )",
