  "logic": "def _random_level(key, max_level: int) -> int:\n    \"\"\"Deterministic random level based on key hash.\"\"\"\n    h = hashlib.sha256(str(key).encode()).digest()\n    level = 1\n    for i in range(max_level - 1):\n        if h[i % len(h)] < 128:\n            level += 1\n        else:\n            break\n    return level",
