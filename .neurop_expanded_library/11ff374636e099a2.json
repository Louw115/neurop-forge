  "logic": "def extract_links(markdown: str) -> list:\n    \"\"\"Extract all links from markdown text.\"\"\"\n    pattern = r'\\[([^\\]]+)\\]\\(([^)]+)\\)'\n    return [{'text': m[0], 'url': m[1]} for m in re.findall(pattern, markdown)]",
