  "logic": "def rot13(text: str) -> str:\n    \"\"\"Apply ROT13 encoding.\"\"\"\n    result = []\n    for c in text:\n        if \"a\" <= c <= \"z\":\n            result.append(chr((ord(c) - ord(\"a\") + 13) % 26 + ord(\"a\")))\n        elif \"A\" <= c <= \"Z\":\n            result.append(chr((ord(c) - ord(\"A\") + 13) % 26 + ord(\"A\")))\n        else:\n            result.append(c)\n    return \"\".join(result)",
