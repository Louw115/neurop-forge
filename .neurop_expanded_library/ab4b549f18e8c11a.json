  "logic": "def hex_to_uuid(v0: str) -> str:\n    v1 = v0.lower()\n    if len(v1) != 32 or not v2((v3 in '0123456789abcdef' for v3 in v1)):\n        return ''\n    return f'{v1[:8]}-{v1[8:12]}-{v1[12:16]}-{v1[16:20]}-{v1[20:]}'",
