  "logic": "def matrix_is_diagonal(m: dict) -> bool:\n    \"\"\"Check if matrix is diagonal.\"\"\"\n    for i in range(m[\"num_rows\"]):\n        for j in range(m[\"num_cols\"]):\n            if i != j and m[\"rows\"][i][j] != 0:\n                return False\n    return True",
