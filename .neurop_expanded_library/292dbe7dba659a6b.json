  "logic": "def calculate_cagr(start_value: float, end_value: float, years: int) -> float:\n    \"\"\"Calculate compound annual growth rate.\"\"\"\n    if start_value <= 0 or years <= 0:\n        return 0.0\n    return ((end_value / start_value) ** (1 / years) - 1) * 100",
