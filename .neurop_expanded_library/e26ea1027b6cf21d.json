  "logic": "def format_prometheus_histogram(v0: dict) -> str:\n    v1 = []\n    v2 = v0['name']\n    v3 = ','.join((f'{v4}=\"{v5}\"' for v4, v5 in v0.get('labels', {}).items()))\n    for v6 in v0['buckets']:\n        v7 = v6['le']\n        v8 = '+Inf' if v7 == float('inf') else str(v7)\n        if v3:\n            v1.append(f'''{v2}_bucket{{{v3},le=\"{v8}\"}} {v6['count']}''')\n        else:\n            v1.append(f'''{v2}_bucket{{le=\"{v8}\"}} {v6['count']}''')\n    if v3:\n        v1.append(f\"{v2}_sum{{{v3}}} {v0['sum']}\")\n        v1.append(f\"{v2}_count{{{v3}}} {v0['count']}\")\n    else:\n        v1.append(f\"{v2}_sum {v0['sum']}\")\n        v1.append(f\"{v2}_count {v0['count']}\")\n    return '\\n'.join(v1)",
