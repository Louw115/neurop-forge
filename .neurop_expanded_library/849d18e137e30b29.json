  "logic": "def aggregate_validation_errors(errors: list) -> dict:\n    \"\"\"Aggregate validation errors by field.\"\"\"\n    by_field = {}\n    for error in errors:\n        field = error.get(\"field\", \"general\")\n        if field not in by_field:\n            by_field[field] = []\n        by_field[field].append(error.get(\"message\"))\n    return by_field",
