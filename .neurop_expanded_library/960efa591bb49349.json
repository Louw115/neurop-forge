  "logic": "def binary_to_text(binary: str) -> str:\n    \"\"\"Convert binary string to text.\"\"\"\n    chars = [binary[i:i+8] for i in range(0, len(binary), 8)]\n    return \"\".join(chr(int(c, 2)) for c in chars)",
