  "logic": "def interpolate_translation(template: str, variables: dict) -> str:\n    \"\"\"Interpolate variables into translation template.\"\"\"\n    result = template\n    for key, value in variables.items():\n        result = result.replace(f\"{{{key}}}\", str(value))\n        result = result.replace(f\"{{{{ {key} }}}}\", str(value))\n    return result",
