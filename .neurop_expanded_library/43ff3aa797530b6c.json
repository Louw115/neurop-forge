  "logic": "def interleave_strings(s1: str, s2: str) -> str:\n    \"\"\"Interleave two strings.\"\"\"\n    result = []\n    for i in range(max(len(s1), len(s2))):\n        if i < len(s1):\n            result.append(s1[i])\n        if i < len(s2):\n            result.append(s2[i])\n    return \"\".join(result)",
