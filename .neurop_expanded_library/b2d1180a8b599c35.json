  "logic": "def saturate(r: int, g: int, b: int, amount: float) -> dict:\n    \"\"\"Increase saturation.\"\"\"\n    hsl = rgb_to_hsl(r, g, b)\n    new_s = min(100, hsl[\"s\"] + amount * 100)\n    return hsl_to_rgb(hsl[\"h\"], round(new_s), hsl[\"l\"])",
