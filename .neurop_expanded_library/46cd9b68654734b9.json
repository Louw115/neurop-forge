  "logic": "def mask_webhook_secret(secret: str) -> str:\n    \"\"\"Mask a webhook secret for display.\"\"\"\n    if len(secret) <= 8:\n        return \"*\" * len(secret)\n    return secret[:4] + \"*\" * (len(secret) - 8) + secret[-4:]",
