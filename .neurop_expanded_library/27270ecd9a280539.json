  "logic": "def get_postgres_type(python_type: str) -> str:\n    \"\"\"Map a Python type name to PostgreSQL type.\"\"\"\n    type_map = {\n        \"str\": \"TEXT\",\n        \"int\": \"INTEGER\",\n        \"float\": \"DOUBLE PRECISION\",\n        \"bool\": \"BOOLEAN\",\n        \"bytes\": \"BYTEA\",\n        \"datetime\": \"TIMESTAMP\",\n        \"date\": \"DATE\",\n        \"time\": \"TIME\",\n        \"timedelta\": \"INTERVAL\",\n        \"Decimal\": \"NUMERIC\",\n        \"dict\": \"JSONB\",\n        \"list\": \"JSONB\",\n        \"uuid\": \"UUID\",\n    }\n    return type_map.get(python_type, \"TEXT\")",
