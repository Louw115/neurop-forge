  "logic": "def atbash_cipher(v0: str) -> str:\n    v1 = []\n    for v2 in v0:\n        if 'a' <= v2 <= 'z':\n            v1.append(v3(v4('z') - (v4(v2) - v4('a'))))\n        elif 'A' <= v2 <= 'Z':\n            v1.append(v3(v4('Z') - (v4(v2) - v4('A'))))\n        else:\n            v1.append(v2)\n    return ''.join(v1)",
