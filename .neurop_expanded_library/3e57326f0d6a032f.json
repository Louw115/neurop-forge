  "logic": "def validate_pool_config(v0: dict) -> dict:\n    v1 = []\n    if v0.get('min_size', 0) < 0:\n        v1.append('min_size must be non-negative')\n    if v0.get('max_size', 0) < v0.get('min_size', 0):\n        v1.append('max_size must be >= min_size')\n    if v0.get('connection_timeout_ms', 0) < 0:\n        v1.append('connection_timeout_ms must be non-negative')\n    if v0.get('idle_timeout_seconds', 0) < 0:\n        v1.append('idle_timeout_seconds must be non-negative')\n    return {'valid': len(v1) == 0, 'errors': v1}",
