  "logic": "def get_pem_type(v0: str) -> str:\n    if not v0:\n        return ''\n    v1 = v0.strip().split('\\n')[0]\n    if v1.startswith('-----BEGIN ') and v1.endswith('-----'):\n        return v1[11:-5]\n    return ''",
