  "logic": "def is_document_file(filename: str) -> bool:\n    \"\"\"Check if file is a document.\"\"\"\n    ext = get_file_extension(filename)\n    return ext in {\"pdf\", \"doc\", \"docx\", \"txt\", \"rtf\", \"odt\", \"xls\", \"xlsx\", \"ppt\", \"pptx\"}",
