  "logic": "def rgb_to_hsl(r: int, g: int, b: int) -> dict:\n    \"\"\"Convert RGB to HSL.\"\"\"\n    r, g, b = r / 255, g / 255, b / 255\n    max_c = max(r, g, b)\n    min_c = min(r, g, b)\n    l = (max_c + min_c) / 2\n    if max_c == min_c:\n        h = s = 0\n    else:\n        d = max_c - min_c\n        s = d / (2 - max_c - min_c) if l > 0.5 else d / (max_c + min_c)\n        if max_c == r:\n            h = (g - b) / d + (6 if g < b else 0)\n        elif max_c == g:\n            h = (b - r) / d + 2\n        else:\n            h = (r - g) / d + 4\n        h /= 6\n    return {\"h\": int(h * 360), \"s\": int(s * 100), \"l\": int(l * 100)}",
