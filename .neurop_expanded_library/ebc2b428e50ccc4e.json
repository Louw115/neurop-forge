  "logic": "def sanitize_table_name(v0: str) -> str:\n    v1 = ''.join((v2 if v2.isalnum() or v2 == '_' else '_' for v2 in v0))\n    if v1 and v1[0].isdigit():\n        v1 = '_' + v1\n    return v1.lower()",
