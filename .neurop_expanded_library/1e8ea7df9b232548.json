  "logic": "def generate_requirements_text(v0: dict) -> str:\n    v1 = []\n    if v0.get('min_length'):\n        v1.append(f\"At least {v0['min_length']} characters\")\n    if v0.get('require_uppercase'):\n        v1.append('One uppercase letter')\n    if v0.get('require_lowercase'):\n        v1.append('One lowercase letter')\n    if v0.get('require_digit'):\n        v1.append('One number')\n    if v0.get('require_special'):\n        v1.append('One special character')\n    return ', '.join(v1)",
