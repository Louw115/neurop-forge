  "logic": "def format_follower_count(v0: int) -> str:\n    if v0 < 1000:\n        return str(v0)\n    if v0 < 1000000:\n        return f'{v0 / 1000:.1f}K'.replace('.0K', 'K')\n    if v0 < 1000000000:\n        return f'{v0 / 1000000:.1f}M'.replace('.0M', 'M')\n    return f'{v0 / 1000000000:.1f}B'.replace('.0B', 'B')",
