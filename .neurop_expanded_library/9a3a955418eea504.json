  "logic": "def parse_duration(duration_str: str) -> int:\n    \"\"\"Parse duration string to seconds.\"\"\"\n    import re\n    total = 0\n    patterns = [\n        (r'(\\d+)\\s*d(?:ays?)?', 86400),\n        (r'(\\d+)\\s*h(?:ours?)?', 3600),\n        (r'(\\d+)\\s*m(?:in(?:utes?)?)?', 60),\n        (r'(\\d+)\\s*s(?:ec(?:onds?)?)?', 1)\n    ]\n    for pattern, multiplier in patterns:\n        match = re.search(pattern, duration_str, re.IGNORECASE)\n        if match:\n            total += int(match.group(1)) * multiplier\n    return total",
