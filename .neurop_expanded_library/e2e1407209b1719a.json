  "logic": "def generate_safe_filename(v0: str, v1: int) -> str:\n    import re\n    v2 = v3.sub('[^\\\\w\\\\-_. ]', '', v0)\n    v2 = v3.sub('\\\\s+', '_', v2)\n    v2 = v2.strip('._')\n    if len(v2) > v1:\n        v4, v5 = v2.rsplit('.', 1) if '.' in v2 else (v2, '')\n        v6 = v1 - len(v5) - 1 if v5 else v1\n        v2 = v4[:v6] + ('.' + v5 if v5 else '')\n    return v2",
