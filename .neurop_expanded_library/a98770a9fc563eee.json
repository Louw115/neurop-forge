  "logic": "def remove_plus_addressing(email: str) -> str:\n    \"\"\"Remove plus addressing from email.\"\"\"\n    if not is_valid_email(email):\n        return email\n    local, domain = email.split('@')\n    local = local.split('+')[0]\n    return f\"{local}@{domain}\"",
