  "logic": "def is_in_blackout(blackouts: list, current_date: str) -> bool:\n    \"\"\"Check if current date is in a blackout period.\"\"\"\n    for blackout in blackouts:\n        if blackout[\"start_date\"] <= current_date <= blackout[\"end_date\"]:\n            return True\n    return False",
