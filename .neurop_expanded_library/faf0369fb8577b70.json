  "logic": "def parse_query_string(query: str) -> dict:\n    \"\"\"Parse a query string into a dictionary.\"\"\"\n    if not query:\n        return {}\n    if query.startswith('?'):\n        query = query[1:]\n    result = {}\n    pairs = query.split('&')\n    for pair in pairs:\n        if '=' in pair:\n            key, value = pair.split('=', 1)\n            result[key] = value\n        elif pair:\n            result[pair] = ''\n    return result",
