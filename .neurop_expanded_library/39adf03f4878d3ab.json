  "logic": "def mask_token(token: str, visible_chars: int) -> str:\n    \"\"\"Mask token showing only last N characters.\"\"\"\n    if len(token) <= visible_chars:\n        return \"*\" * len(token)\n    return \"*\" * (len(token) - visible_chars) + token[-visible_chars:]",
