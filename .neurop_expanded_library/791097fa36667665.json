  "logic": "def average_value(items: list) -> float:\n    \"\"\"Calculate the average of numeric values in a list.\"\"\"\n    numbers = [item for item in items if isinstance(item, (int, float))]\n    if not numbers:\n        return 0.0\n    return sum(numbers) / len(numbers)",
