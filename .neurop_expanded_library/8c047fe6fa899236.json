  "logic": "def char_frequency(text: str) -> dict:\n    \"\"\"Get frequency of each ASCII character.\"\"\"\n    freq = {}\n    for c in text:\n        if is_ascii(c):\n            freq[c] = freq.get(c, 0) + 1\n    return freq",
