  "logic": "def remaining_ttl(created_at: int, ttl_seconds: int, current_time: int) -> int:\n    \"\"\"Calculate remaining TTL in seconds.\"\"\"\n    remaining = (created_at + ttl_seconds) - current_time\n    return max(0, remaining)",
