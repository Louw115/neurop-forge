  "logic": "def generalize_age(age: int, bucket_size: int) -> str:\n    \"\"\"Generalize age to bucket.\"\"\"\n    lower = (age // bucket_size) * bucket_size\n    upper = lower + bucket_size - 1\n    return f\"{lower}-{upper}\"",
