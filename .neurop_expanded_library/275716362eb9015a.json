  "logic": "def parse_duration(v0: str) -> int:\n    import re\n    v1 = 0\n    v2 = [('(\\\\d+)\\\\s*d(?:ays?)?', 86400), ('(\\\\d+)\\\\s*h(?:ours?)?', 3600), ('(\\\\d+)\\\\s*m(?:in(?:utes?)?)?', 60), ('(\\\\d+)\\\\s*s(?:ec(?:onds?)?)?', 1)]\n    for v3, v4 in v2:\n        v5 = v6.search(v3, v0, v6.IGNORECASE)\n        if v5:\n            v1 += int(v5.group(1)) * v4\n    return v1",
