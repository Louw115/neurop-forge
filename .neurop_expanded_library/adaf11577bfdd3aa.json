  "logic": "def generate_test_phone(v0: int) -> str:\n    import hashlib\n    v1 = v2.sha256(f'phone:{v0}'.encode()).hexdigest()\n    v3 = ''.join((v4 for v4 in v1 if v4.isdigit()))[:10]\n    return f'+1{v3}'",
