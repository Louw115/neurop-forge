  "logic": "def normalize_path(path: str) -> str:\n    \"\"\"Normalize URI path.\"\"\"\n    segments = path.split(\"/\")\n    result = []\n    for seg in segments:\n        if seg == \"..\":\n            if result and result[-1] != \"\":\n                result.pop()\n        elif seg != \".\":\n            result.append(seg)\n    return \"/\".join(result)",
