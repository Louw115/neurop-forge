  "logic": "def round_to_hour(time: dict) -> dict:\n    \"\"\"Round time to nearest hour.\"\"\"\n    total_minutes = time[\"hours\"] * 60 + time[\"minutes\"]\n    if time[\"minutes\"] >= 30:\n        total_minutes = (time[\"hours\"] + 1) * 60\n    return {\"hours\": (total_minutes // 60) % 24, \"minutes\": 0, \"seconds\": 0}",
