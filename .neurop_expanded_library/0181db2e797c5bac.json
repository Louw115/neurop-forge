  "logic": "def days_in_month(year: int, month: int) -> int:\n    \"\"\"Get number of days in a month.\"\"\"\n    days = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31]\n    if month == 2 and is_leap_year(year):\n        return 29\n    return days[month - 1] if 1 <= month <= 12 else 0",
