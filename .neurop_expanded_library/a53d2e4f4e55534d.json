  "logic": "def refill_tokens(v0: dict, v1: int) -> dict:\n    v2 = v1 - v0['last_refill']\n    v3 = v2 // v0['refill_interval_ms']\n    v4 = min(v0['capacity'], v0['tokens'] + v3 * v0['refill_rate'])\n    return {**v0, 'tokens': v4, 'last_refill': v1}",
