  "logic": "def build_filter_query_string(v0: list) -> str:\n    v1 = []\n    for v2 in v0:\n        v3 = v2.get('field', '')\n        v4 = v2.get('operator', 'eq')\n        v5 = v2.get('value', '')\n        if v4 == 'eq':\n            v1.append(f'filter[{v3}]={v5}')\n        else:\n            v1.append(f'filter[{v3}__{v4}]={v5}')\n    return '&'.join(v1)",
