  "logic": "def build_security_headers(v0: str, v1: int, v2: str, v3: bool, v4: bool) -> dict:\n    v5 = {}\n    if v0:\n        v5['Content-Security-Policy'] = v0\n    if v1 > 0:\n        v5['Strict-Transport-Security'] = f'max-age={v1}; includeSubDomains'\n    if v2:\n        v5['X-Frame-Options'] = v2\n    if v3:\n        v5['X-Content-Type-Options'] = 'nosniff'\n    if v4:\n        v5['X-XSS-Protection'] = '1; mode=block'\n    return v5",
