  "logic": "def validate_pattern(value: str, pattern: str, message: str) -> dict:\n    \"\"\"Validate against regex pattern.\"\"\"\n    import re\n    is_valid = bool(re.match(pattern, str(value)))\n    return {\"valid\": is_valid, \"error\": None if is_valid else message}",
