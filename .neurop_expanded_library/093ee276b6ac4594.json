  "logic": "def sanitize_phone_input(phone: str) -> str:\n    \"\"\"Sanitize phone input by removing invalid characters.\"\"\"\n    allowed = set('0123456789+()-. ')\n    return ''.join(c for c in phone if c in allowed)",
