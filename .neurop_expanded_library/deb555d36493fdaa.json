  "logic": "def has_subquery(v0: str) -> bool:\n    import re\n    v1 = v2.sub(\"'[^']*'\", '', v0)\n    v3 = v2.findall('\\\\([^()]+\\\\)', v1)\n    for v4 in v3:\n        if v2.search('\\\\bSELECT\\\\b', v4, v2.IGNORECASE):\n            return True\n    return False",
