  "logic": "def url_decode(encoded: str) -> str:\n    \"\"\"URL-decode a string.\"\"\"\n    if not encoded:\n        return \"\"\n    result = []\n    i = 0\n    while i < len(encoded):\n        if encoded[i] == '%' and i + 2 < len(encoded):\n            try:\n                byte_val = int(encoded[i+1:i+3], 16)\n                result.append(chr(byte_val))\n                i += 3\n            except ValueError:\n                result.append(encoded[i])\n                i += 1\n        elif encoded[i] == '+':\n            result.append(' ')\n            i += 1\n        else:\n            result.append(encoded[i])\n            i += 1\n    return ''.join(result)",
