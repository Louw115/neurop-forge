  "logic": "def longest_common_suffix(strings: list) -> str:\n    \"\"\"Find longest common suffix of strings.\"\"\"\n    if not strings:\n        return \"\"\n    suffix = strings[0]\n    for s in strings[1:]:\n        while not s.endswith(suffix):\n            suffix = suffix[1:]\n            if not suffix:\n                return \"\"\n    return suffix",
