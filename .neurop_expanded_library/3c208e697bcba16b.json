  "logic": "def format_semver(major: int, minor: int, patch: int, prerelease: str, build: str) -> str:\n    \"\"\"Format a semantic version string.\"\"\"\n    version = f\"{major}.{minor}.{patch}\"\n    if prerelease:\n        version += f\"-{prerelease}\"\n    if build:\n        version += f\"+{build}\"\n    return version",
