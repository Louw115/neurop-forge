  "logic": "def calculate_prefetch_count(avg_processing_time_ms: int, target_latency_ms: int) -> int:\n    \"\"\"Calculate optimal prefetch count.\"\"\"\n    if avg_processing_time_ms <= 0:\n        return 1\n    return max(1, target_latency_ms // avg_processing_time_ms)",
