  "logic": "def calculate_audit_statistics(v0: list) -> dict:\n    v1 = len(v0)\n    v2 = {}\n    v3 = {}\n    v4 = {}\n    for v5 in v0:\n        v6 = v5.get('operation', 'UNKNOWN')\n        v7 = v5.get('table_name', 'UNKNOWN')\n        v8 = v5.get('user_id', 'UNKNOWN')\n        v2[v6] = v2.get(v6, 0) + 1\n        v3[v7] = v3.get(v7, 0) + 1\n        v4[v8] = v4.get(v8, 0) + 1\n    return {'total_records': v1, 'by_operation': v2, 'by_table': v3, 'by_user': v4}",
