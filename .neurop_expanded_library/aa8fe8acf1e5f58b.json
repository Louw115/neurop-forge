  "logic": "def isbn13_checksum(digits: str) -> str:\n    \"\"\"Calculate ISBN-13 check digit.\"\"\"\n    total = 0\n    for i, d in enumerate(digits[:12]):\n        weight = 1 if i % 2 == 0 else 3\n        total += int(d) * weight\n    return str((10 - (total % 10)) % 10)",
