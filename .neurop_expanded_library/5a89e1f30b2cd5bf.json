  "logic": "def json_add(builder: dict, key: str, value) -> dict:\n    \"\"\"Add key-value to current object.\"\"\"\n    stack = list(builder[\"stack\"])\n    if isinstance(stack[-1], dict):\n        stack[-1] = {**stack[-1], key: value}\n    return {**builder, \"stack\": stack}",
