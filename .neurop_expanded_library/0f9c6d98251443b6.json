  "logic": "def interval_union(a: dict, b: dict) -> dict:\n    \"\"\"Get union of two overlapping intervals.\"\"\"\n    if not intervals_overlap(a, b) and not intervals_touch(a, b):\n        return None\n    return create_interval(min(a[\"start\"], b[\"start\"]), max(a[\"end\"], b[\"end\"]))",
