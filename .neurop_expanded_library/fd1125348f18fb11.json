  "logic": "def consume_tokens(bucket: dict, count: int) -> dict:\n    \"\"\"Consume tokens from bucket.\"\"\"\n    if bucket[\"tokens\"] >= count:\n        return {\"success\": True, \"bucket\": {**bucket, \"tokens\": bucket[\"tokens\"] - count}}\n    return {\"success\": False, \"bucket\": bucket}",
