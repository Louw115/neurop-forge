  "logic": "def extract_all_between(text: str, start_marker: str, end_marker: str) -> list:\n    \"\"\"Extract all texts between markers.\"\"\"\n    results = []\n    start = 0\n    while True:\n        start_pos = text.find(start_marker, start)\n        if start_pos == -1:\n            break\n        start_pos += len(start_marker)\n        end_pos = text.find(end_marker, start_pos)\n        if end_pos == -1:\n            break\n        results.append(text[start_pos:end_pos])\n        start = end_pos + len(end_marker)\n    return results",
