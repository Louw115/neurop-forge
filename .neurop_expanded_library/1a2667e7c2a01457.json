  "logic": "def singularize_resource(name: str) -> str:\n    \"\"\"Singularize a resource name.\"\"\"\n    if name.endswith('ies'):\n        return name[:-3] + 'y'\n    if name.endswith('es') and len(name) > 3 and name[-3] in 'sxz':\n        return name[:-2]\n    if name.endswith('s') and not name.endswith('ss'):\n        return name[:-1]\n    return name",
