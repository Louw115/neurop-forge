  "logic": "def format_unified_diff(diff: list, context_lines: int) -> str:\n    \"\"\"Format diff as unified diff.\"\"\"\n    lines = []\n    for op in diff:\n        if op[\"op\"] == \"equal\":\n            lines.append(f\" {op['value']}\")\n        elif op[\"op\"] == \"insert\":\n            lines.append(f\"+{op['value']}\")\n        elif op[\"op\"] == \"delete\":\n            lines.append(f\"-{op['value']}\")\n    return \"\\n\".join(lines)",
