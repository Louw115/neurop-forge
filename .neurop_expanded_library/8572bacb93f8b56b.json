  "logic": "def pluralize_resource(name: str) -> str:\n    \"\"\"Pluralize a resource name for collection endpoints.\"\"\"\n    if name.endswith('y') and len(name) > 1 and name[-2] not in 'aeiou':\n        return name[:-1] + 'ies'\n    if name.endswith(('s', 'x', 'z', 'ch', 'sh')):\n        return name + 'es'\n    return name + 's'",
