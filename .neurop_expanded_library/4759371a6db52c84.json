  "logic": "def hex_to_rgb(hex_color: str) -> dict:\n    \"\"\"Convert hex color to RGB.\"\"\"\n    hex_color = hex_color.lstrip(\"#\")\n    if len(hex_color) == 3:\n        hex_color = \"\".join(c * 2 for c in hex_color)\n    r = int(hex_color[0:2], 16)\n    g = int(hex_color[2:4], 16)\n    b = int(hex_color[4:6], 16)\n    return {\"r\": r, \"g\": g, \"b\": b}",
