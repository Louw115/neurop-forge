  "logic": "def paginate_results(results: list, page: int, per_page: int) -> dict:\n    \"\"\"Paginate search results.\"\"\"\n    total = len(results)\n    start = (page - 1) * per_page\n    end = start + per_page\n    items = results[start:end]\n    return {\n        \"items\": items,\n        \"total\": total,\n        \"page\": page,\n        \"per_page\": per_page,\n        \"total_pages\": (total + per_page - 1) // per_page if per_page > 0 else 0\n    }",
