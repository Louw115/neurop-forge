  "logic": "def get_package_type(v0: float, v1: float, v2: float) -> str:\n    v3 = max(v0, v1, v2)\n    v4 = min(v0, v1, v2)\n    if v3 <= 6 and v4 <= 4:\n        return 'small'\n    if v3 <= 12 and v4 <= 8:\n        return 'medium'\n    if v3 <= 24 and v4 <= 16:\n        return 'large'\n    return 'oversized'",
