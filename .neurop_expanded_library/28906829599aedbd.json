  "logic": "def parse_full_name(name: str) -> dict:\n    \"\"\"Parse full name into parts.\"\"\"\n    parts = name.strip().split()\n    if len(parts) == 0:\n        return {\"first\": \"\", \"last\": \"\"}\n    if len(parts) == 1:\n        return {\"first\": parts[0], \"last\": \"\"}\n    return {\"first\": parts[0], \"last\": \" \".join(parts[1:])}",
