  "logic": "def is_valid_uuid(text: str) -> bool:\n    \"\"\"Check if a string is a valid UUID (any version).\"\"\"\n    pattern = r'^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$'\n    return bool(re.match(pattern, text))",
