  "logic": "def get_index_method(sql_type: str) -> str:\n    \"\"\"Get the recommended index method for a SQL type.\"\"\"\n    if is_json_type(sql_type):\n        return \"GIN\"\n    if is_text_type(sql_type):\n        return \"BTREE\"\n    if is_array_type(sql_type):\n        return \"GIN\"\n    return \"BTREE\"",
