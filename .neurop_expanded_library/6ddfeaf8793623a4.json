  "logic": "def format_uptime(seconds: int) -> str:\n    \"\"\"Format uptime in human-readable form.\"\"\"\n    days = seconds // 86400\n    hours = (seconds % 86400) // 3600\n    minutes = (seconds % 3600) // 60\n    if days > 0:\n        return f\"{days}d {hours}h {minutes}m\"\n    if hours > 0:\n        return f\"{hours}h {minutes}m\"\n    return f\"{minutes}m\"",
