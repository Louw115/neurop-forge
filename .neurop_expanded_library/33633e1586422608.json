  "logic": "def bloom_add(bloom: dict, item: str) -> dict:\n    \"\"\"Add item to bloom filter.\"\"\"\n    bits = list(bloom[\"bits\"])\n    for i in range(bloom[\"num_hashes\"]):\n        idx = _hash_item(item, i, bloom[\"size\"])\n        bits[idx] = 1\n    return {\n        \"bits\": bits,\n        \"size\": bloom[\"size\"],\n        \"num_hashes\": bloom[\"num_hashes\"],\n        \"count\": bloom[\"count\"] + 1\n    }",
