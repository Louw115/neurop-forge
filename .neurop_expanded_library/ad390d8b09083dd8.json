  "logic": "def mask_ssn(v0: str) -> str:\n    v1 = ''.join((v2 for v2 in v0 if v2.isdigit()))\n    if len(v1) >= 4:\n        return f'***-**-{v1[-4:]}'\n    return '***-**-****'",
