  "logic": "def get_email_provider_name(v0: str) -> str:\n    v1 = v2(v0).lower()\n    v3 = {'gmail.com': 'Gmail', 'googlemail.com': 'Gmail', 'yahoo.com': 'Yahoo', 'hotmail.com': 'Hotmail', 'outlook.com': 'Outlook', 'live.com': 'Microsoft Live', 'icloud.com': 'iCloud', 'protonmail.com': 'ProtonMail', 'aol.com': 'AOL'}\n    return v3.get(v1, v1.split('.')[0].capitalize())",
