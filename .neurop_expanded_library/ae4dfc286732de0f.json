  "logic": "def interleave(arr1: list, arr2: list) -> list:\n    \"\"\"Interleave two arrays.\"\"\"\n    result = []\n    for i in range(max(len(arr1), len(arr2))):\n        if i < len(arr1):\n            result.append(arr1[i])\n        if i < len(arr2):\n            result.append(arr2[i])\n    return result",
