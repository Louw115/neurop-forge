  "logic": "def remove_suffix(slug: str, suffix: str, separator: str) -> str:\n    \"\"\"Remove suffix from slug.\"\"\"\n    full_suffix = f\"{separator}{suffix}\"\n    if slug.endswith(full_suffix):\n        return slug[:-len(full_suffix)]\n    return slug",
