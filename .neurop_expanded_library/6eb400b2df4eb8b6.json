  "logic": "def cmyk_to_rgb(c: int, m: int, y: int, k: int) -> dict:\n    \"\"\"Convert CMYK to RGB.\"\"\"\n    r = round(255 * (1 - c / 100) * (1 - k / 100))\n    g = round(255 * (1 - m / 100) * (1 - k / 100))\n    b = round(255 * (1 - y / 100) * (1 - k / 100))\n    return {\"r\": r, \"g\": g, \"b\": b}",
