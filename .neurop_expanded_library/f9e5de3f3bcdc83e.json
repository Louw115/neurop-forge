  "logic": "def wrap_text(text: str, width: int) -> list:\n    \"\"\"Wrap text to width.\"\"\"\n    words = text.split()\n    lines = []\n    current = []\n    length = 0\n    for word in words:\n        if length + len(word) + len(current) <= width:\n            current.append(word)\n            length += len(word)\n        else:\n            if current:\n                lines.append(' '.join(current))\n            current = [word]\n            length = len(word)\n    if current:\n        lines.append(' '.join(current))\n    return lines",
