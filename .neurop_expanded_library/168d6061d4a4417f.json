  "logic": "def sanitize_json_string(text: str) -> str:\n    \"\"\"Sanitize string for JSON embedding.\"\"\"\n    text = text.replace(\"\\\\\", \"\\\\\\\\\")\n    text = text.replace('\"', '\\\\\"')\n    text = text.replace(\"\\n\", \"\\\\n\")\n    text = text.replace(\"\\r\", \"\\\\r\")\n    text = text.replace(\"\\t\", \"\\\\t\")\n    return text",
