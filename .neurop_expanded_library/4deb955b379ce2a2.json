  "logic": "def record_call(v0: dict, v1: int) -> dict:\n    v2 = v1 - v0['window_ms']\n    v3 = [v4 for v4 in v0['calls'] if v4 >= v2]\n    v3.append(v1)\n    return {**v0, 'calls': v3}",
