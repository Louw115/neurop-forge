  "logic": "def get_error_severity(status_code: int) -> str:\n    \"\"\"Get error severity level based on status code.\"\"\"\n    if status_code < 400:\n        return \"info\"\n    if status_code < 500:\n        return \"warning\"\n    if status_code < 503:\n        return \"error\"\n    return \"critical\"",
