  "logic": "def extract_headings(markdown: str) -> list:\n    \"\"\"Extract all headings from markdown text.\"\"\"\n    pattern = r'^(#{1,6})\\s+(.+)$'\n    return [{'level': len(m[0]), 'text': m[1].strip()} for m in re.findall(pattern, markdown, re.MULTILINE)]",
