  "logic": "def add_class(attrs: dict, class_name: str) -> dict:\n    \"\"\"Add class to attributes.\"\"\"\n    result = dict(attrs)\n    existing = result.get(\"class\", \"\")\n    if existing:\n        result[\"class\"] = f\"{existing} {class_name}\"\n    else:\n        result[\"class\"] = class_name\n    return result",
