  "logic": "def one_hot_decode(encoded: list, categories: list) -> str:\n    \"\"\"Decode a one-hot encoded value.\"\"\"\n    for i, val in enumerate(encoded):\n        if val == 1 and i < len(categories):\n            return categories[i]\n    return \"\"",
