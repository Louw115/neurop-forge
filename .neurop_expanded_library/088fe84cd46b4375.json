  "logic": "def word_frequency(text: str) -> dict:\n    \"\"\"Calculate the frequency of each word in a text.\"\"\"\n    if not text:\n        return {}\n    words = text.lower().split()\n    freq = {}\n    for word in words:\n        cleaned = ''.join(c for c in word if c.isalnum())\n        if cleaned:\n            freq[cleaned] = freq.get(cleaned, 0) + 1\n    return freq",
