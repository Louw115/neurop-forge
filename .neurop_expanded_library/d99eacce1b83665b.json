  "logic": "def format_duration_short(seconds: int) -> str:\n    \"\"\"Format duration in short form.\"\"\"\n    if seconds < 60:\n        return f\"{seconds}s\"\n    elif seconds < 3600:\n        mins = seconds // 60\n        secs = seconds % 60\n        return f\"{mins}m {secs}s\" if secs else f\"{mins}m\"\n    else:\n        hours = seconds // 3600\n        mins = (seconds % 3600) // 60\n        return f\"{hours}h {mins}m\" if mins else f\"{hours}h\"",
