  "logic": "def is_valid_mx_domain_format(v0: str) -> bool:\n    if not v0 or '.' not in v0:\n        return False\n    v1 = v0.split('.')\n    return v2((v3.match('^[a-zA-Z0-9]([a-zA-Z0-9-]*[a-zA-Z0-9])?$', v4) for v4 in v1))",
