  "logic": "def to_snakecase(v0: str) -> str:\n    v0 = v1.sub('([A-Z]+)([A-Z][a-z])', '\\\\1_\\\\2', v0)\n    v0 = v1.sub('([a-z\\\\d])([A-Z])', '\\\\1_\\\\2', v0)\n    v0 = v1.sub('[\\\\s\\\\-]+', '_', v0)\n    return v0.lower()",
