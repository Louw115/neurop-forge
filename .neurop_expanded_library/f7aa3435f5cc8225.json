  "logic": "def validate_numeric_range(v0, v1, v2, v3: str) -> dict:\n    if v1 is not None and v0 < v1:\n        return {'valid': False, 'error': f'{v3} must be at least {v1}'}\n    if v2 is not None and v0 > v2:\n        return {'valid': False, 'error': f'{v3} must be at most {v2}'}\n    return {'valid': True, 'error': None}",
