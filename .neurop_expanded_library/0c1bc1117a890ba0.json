  "logic": "def slugify(text: str) -> str:\n    \"\"\"Convert a string to a URL-friendly slug.\"\"\"\n    if not text:\n        return \"\"\n    result = []\n    for char in text.lower():\n        if char.isalnum():\n            result.append(char)\n        elif char in ' -_':\n            if result and result[-1] != '-':\n                result.append('-')\n    slug = ''.join(result)\n    return slug.strip('-')",
