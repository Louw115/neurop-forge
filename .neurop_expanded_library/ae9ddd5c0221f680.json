  "logic": "def validate_mac_address(mac: str) -> bool:\n    \"\"\"Validate MAC address.\"\"\"\n    import re\n    patterns = [\n        r'^([0-9A-Fa-f]{2}:){5}[0-9A-Fa-f]{2}$',\n        r'^([0-9A-Fa-f]{2}-){5}[0-9A-Fa-f]{2}$',\n        r'^[0-9A-Fa-f]{12}$'\n    ]\n    return any(re.match(p, mac) for p in patterns)",
