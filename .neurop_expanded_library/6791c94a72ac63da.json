  "logic": "def compare_periods(v0: float, v1: float) -> dict:\n    v2 = v0 - v1\n    v3 = v2 / v1 * 100 if v1 != 0 else 0\n    v4 = 'up' if v2 > 0 else 'down' if v2 < 0 else 'flat'\n    return {'current': v0, 'previous': v1, 'change': v2, 'change_percent': v3, 'trend': v4}",
