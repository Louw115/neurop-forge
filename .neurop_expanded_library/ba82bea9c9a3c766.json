  "logic": "def format_relative_time(seconds_ago: int) -> str:\n    \"\"\"Format time as relative (2h ago, etc.).\"\"\"\n    if seconds_ago < 60:\n        return \"just now\"\n    if seconds_ago < 3600:\n        mins = seconds_ago // 60\n        return f\"{mins}m ago\"\n    if seconds_ago < 86400:\n        hours = seconds_ago // 3600\n        return f\"{hours}h ago\"\n    if seconds_ago < 604800:\n        days = seconds_ago // 86400\n        return f\"{days}d ago\"\n    if seconds_ago < 2592000:\n        weeks = seconds_ago // 604800\n        return f\"{weeks}w ago\"\n    if seconds_ago < 31536000:\n        months = seconds_ago // 2592000\n        return f\"{months}mo ago\"\n    years = seconds_ago // 31536000\n    return f\"{years}y ago\"",
