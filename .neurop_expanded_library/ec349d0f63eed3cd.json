  "logic": "def pluralize_word(word: str, count: int) -> str:\n    \"\"\"Pluralize a word based on count.\"\"\"\n    if count == 1:\n        return word\n    if word.endswith('y') and len(word) > 1 and word[-2] not in 'aeiou':\n        return word[:-1] + 'ies'\n    if word.endswith(('s', 'x', 'z', 'ch', 'sh')):\n        return word + 'es'\n    return word + 's'",
