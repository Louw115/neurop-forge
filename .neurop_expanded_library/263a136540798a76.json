  "logic": "def from_roman(roman: str) -> int:\n    \"\"\"Convert Roman numerals to integer.\"\"\"\n    values = {\"I\": 1, \"V\": 5, \"X\": 10, \"L\": 50, \"C\": 100, \"D\": 500, \"M\": 1000}\n    result = 0\n    prev = 0\n    for char in reversed(roman.upper()):\n        value = values.get(char, 0)\n        if value < prev:\n            result -= value\n        else:\n            result += value\n        prev = value\n    return result",
