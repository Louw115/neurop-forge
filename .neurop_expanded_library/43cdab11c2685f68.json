  "logic": "def add_days(year: int, month: int, day: int, days_to_add: int) -> dict:\n    \"\"\"Add days to a date.\"\"\"\n    total_days = day + days_to_add\n    while total_days > days_in_month(year, month):\n        total_days -= days_in_month(year, month)\n        month += 1\n        if month > 12:\n            month = 1\n            year += 1\n    while total_days < 1:\n        month -= 1\n        if month < 1:\n            month = 12\n            year -= 1\n        total_days += days_in_month(year, month)\n    return {\"year\": year, \"month\": month, \"day\": total_days}",
