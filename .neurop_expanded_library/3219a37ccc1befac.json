  "logic": "def partition_jobs_by_type(jobs: list) -> dict:\n    \"\"\"Partition jobs by type for parallel processing.\"\"\"\n    result = {}\n    for job in jobs:\n        job_type = job.get(\"type\", \"default\")\n        if job_type not in result:\n            result[job_type] = []\n        result[job_type].append(job)\n    return result",
