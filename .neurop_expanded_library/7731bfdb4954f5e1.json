  "logic": "def slugify_unicode(text: str) -> str:\n    \"\"\"Convert text to slug preserving unicode letters.\"\"\"\n    text = text.lower().strip()\n    text = re.sub(r'[^\\w\\s-]', '', text, flags=re.UNICODE)\n    text = re.sub(r'[-\\s]+', '-', text)\n    return text.strip('-')",
