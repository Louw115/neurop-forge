  "logic": "def is_valid_uuid_v4(value: str) -> bool:\n    \"\"\"Check if a string is a valid UUID v4.\"\"\"\n    pattern = r'^[0-9a-f]{8}-[0-9a-f]{4}-4[0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$'\n    return bool(re.match(pattern, value.lower()))",
