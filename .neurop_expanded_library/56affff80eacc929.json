  "logic": "def get_status_category(code: int) -> str:\n    \"\"\"Get the category of an HTTP status code.\"\"\"\n    if 100 <= code < 200:\n        return \"informational\"\n    if 200 <= code < 300:\n        return \"success\"\n    if 300 <= code < 400:\n        return \"redirection\"\n    if 400 <= code < 500:\n        return \"client_error\"\n    if 500 <= code < 600:\n        return \"server_error\"\n    return \"unknown\"",
