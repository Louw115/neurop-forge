  "logic": "def format_duration_milliseconds(ms: int) -> str:\n    \"\"\"Format milliseconds as human-readable duration.\"\"\"\n    if ms < 0:\n        return \"0ms\"\n    if ms < 1000:\n        return f\"{ms}ms\"\n    return format_duration_seconds(ms // 1000)",
