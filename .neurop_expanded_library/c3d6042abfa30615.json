  "logic": "def vector_equals(v1: dict, v2: dict, tolerance: float) -> bool:\n    \"\"\"Check if two vectors are equal within tolerance.\"\"\"\n    if v1[\"dimension\"] != v2[\"dimension\"]:\n        return False\n    return all(abs(a - b) <= tolerance for a, b in zip(v1[\"values\"], v2[\"values\"]))",
