  "logic": "def validate_content_type(v0: str, v1: list) -> dict:\n    if not v0:\n        return {'valid': False, 'error': 'Content-Type header required'}\n    v2 = v0.split(';')[0].strip().lower()\n    if v2 not in [v3.lower() for v3 in v1]:\n        return {'valid': False, 'error': f\"Unsupported Content-Type. Expected: {', '.join(v1)}\"}\n    return {'valid': True, 'error': None}",
