  "logic": "def sort_table_data(data: list, sort_key: str, sort_direction: str) -> list:\n    \"\"\"Sort table data by column.\"\"\"\n    reverse = sort_direction.lower() == \"desc\"\n    return sorted(data, key=lambda x: x.get(sort_key, \"\"), reverse=reverse)",
