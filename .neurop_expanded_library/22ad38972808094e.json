  "logic": "def mask_uuid(v0: str, v1: int) -> str:\n    v2 = v3(v0)\n    if not v2:\n        return ''\n    if v1 <= 0:\n        return '********-****-****-****-************'\n    v4 = v2[:v1]\n    v5 = v2[v1:].replace('-', '*').replace('a', '*').replace('b', '*').replace('c', '*').replace('d', '*').replace('e', '*').replace('f', '*')\n    for v6 in '0123456789':\n        v5 = v5.replace(v6, '*')\n    v7 = v4 + v5\n    return v7[:8] + '-' + v7[8:12].replace('-', '') + '-' + v7[12:16].replace('-', '') + '-' + v7[16:20].replace('-', '') + '-' + v7[20:].replace('-', '')",
