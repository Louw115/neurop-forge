  "logic": "def calculate_parallel_taxes(amount: float, rates: list) -> dict:\n    \"\"\"Calculate parallel taxes (each on subtotal).\"\"\"\n    taxes = [round(amount * rate / 100, 2) for rate in rates]\n    return {\n        \"subtotal\": amount,\n        \"taxes\": taxes,\n        \"total\": round(amount + sum(taxes), 2)\n    }",
