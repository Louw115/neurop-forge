  "logic": "def validate_form_field(value: str, required: bool, min_length: int, max_length: int, pattern: str) -> dict:\n    \"\"\"Validate a form field.\"\"\"\n    errors = []\n    if required and not value:\n        errors.append(\"This field is required\")\n    elif value:\n        if min_length > 0 and len(value) < min_length:\n            errors.append(f\"Minimum {min_length} characters required\")\n        if max_length > 0 and len(value) > max_length:\n            errors.append(f\"Maximum {max_length} characters allowed\")\n        if pattern:\n            import re\n            if not re.match(pattern, value):\n                errors.append(\"Invalid format\")\n    return {\"valid\": len(errors) == 0, \"errors\": errors}",
