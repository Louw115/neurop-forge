  "logic": "def strip_tag(v0: str, v1: str) -> str:\n    v2 = f'<{v1}[^>]*>.*?</{v1}>'\n    return v3.sub(v2, '', v0, flags=v3.DOTALL | v3.IGNORECASE)",
