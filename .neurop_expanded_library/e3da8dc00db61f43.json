  "logic": "def split_frontmatter(v0: str) -> dict:\n    v1 = '^---\\\\n(.*?)\\\\n---\\\\n(.*)$'\n    v2 = v3.match(v1, v0, v3.DOTALL)\n    if v2:\n        return {'frontmatter': v2.group(1), 'content': v2.group(2)}\n    return {'frontmatter': '', 'content': v0}",
