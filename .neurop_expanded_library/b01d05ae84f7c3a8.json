  "logic": "def validate_array_items(v0: list, v1: str, v2: str) -> dict:\n    v3 = {'string': str, 'integer': int, 'number': (int, float), 'boolean': bool}\n    v4 = v3.get(v1)\n    if not v4:\n        return {'valid': True, 'error': None}\n    for v5, v6 in enumerate(v0):\n        if not isinstance(v6, v4):\n            return {'valid': False, 'error': f'{v2}[{v5}] must be {v1}'}\n    return {'valid': True, 'error': None}",
