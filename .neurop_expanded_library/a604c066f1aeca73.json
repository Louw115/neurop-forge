  "logic": "def parse_jwt_payload(token: str) -> dict:\n    \"\"\"Parse the payload portion of a JWT (without verification).\"\"\"\n    import base64\n    import json\n    try:\n        parts = token.split('.')\n        if len(parts) != 3:\n            return {}\n        payload = parts[1]\n        padding = 4 - len(payload) % 4\n        if padding != 4:\n            payload += '=' * padding\n        decoded = base64.urlsafe_b64decode(payload)\n        return json.loads(decoded)\n    except Exception:\n        return {}",
