  "logic": "def close_tag(builder: dict, tag: str) -> dict:\n    \"\"\"Close an HTML tag.\"\"\"\n    indent = \"  \" * max(0, builder.get(\"indent\", 1) - 1)\n    text = f\"{indent}</{tag}>\\n\"\n    return {\n        \"parts\": builder[\"parts\"] + [text],\n        \"length\": builder[\"length\"] + len(text),\n        \"indent\": max(0, builder.get(\"indent\", 1) - 1)\n    }",
