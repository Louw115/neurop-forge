  "logic": "def extract_table_names(v0: str) -> list:\n    import re\n    v1 = set()\n    v2 = v3.findall('\\\\bFROM\\\\s+([^\\\\s,;()]+)', v0, v3.IGNORECASE)\n    v1.update(v2)\n    v4 = v3.findall('\\\\bJOIN\\\\s+([^\\\\s,;()]+)', v0, v3.IGNORECASE)\n    v1.update(v4)\n    v5 = v3.findall('\\\\bINTO\\\\s+([^\\\\s,;()]+)', v0, v3.IGNORECASE)\n    v1.update(v5)\n    v6 = v3.findall('\\\\bUPDATE\\\\s+([^\\\\s,;()]+)', v0, v3.IGNORECASE)\n    v1.update(v6)\n    v7 = {v8.strip('\"').strip(\"'\") for v8 in v1}\n    v9 = {'SELECT', 'SET', 'VALUES', 'WHERE', 'AND', 'OR'}\n    return [v8 for v8 in v7 if v8.upper() not in v9]",
