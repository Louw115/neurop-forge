  "logic": "def apply_full_jitter(max_delay: float, seed: int) -> float:\n    \"\"\"Apply full jitter (0 to max_delay).\"\"\"\n    h = hashlib.sha256(str(seed).encode()).digest()\n    return max_delay * (h[0] / 255.0)",
