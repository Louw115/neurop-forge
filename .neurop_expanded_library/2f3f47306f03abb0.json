  "logic": "def html_decode(encoded: str) -> str:\n    \"\"\"HTML decode a string.\"\"\"\n    return (encoded\n        .replace(\"&amp;\", \"&\")\n        .replace(\"&lt;\", \"<\")\n        .replace(\"&gt;\", \">\")\n        .replace(\"&quot;\", '\"')\n        .replace(\"&#x27;\", \"'\"))",
