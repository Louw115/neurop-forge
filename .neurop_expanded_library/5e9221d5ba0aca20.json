  "logic": "def check_uuid_format(v0: str, v1: str) -> dict:\n    import re\n    v2 = '^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$'\n    if not v3.match(v2, v0.lower()):\n        return {'valid': False, 'error': f'{v1} must be a valid UUID'}\n    return {'valid': True, 'error': None}",
