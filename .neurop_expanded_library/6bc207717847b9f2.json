  "logic": "def contains_special_char(text: str) -> bool:\n    \"\"\"Check if a string contains at least one special character.\"\"\"\n    special_chars = \"!@#$%^&*()_+-=[]{}|;':\\\",./<>?\"\n    return any(c in special_chars for c in text)",
