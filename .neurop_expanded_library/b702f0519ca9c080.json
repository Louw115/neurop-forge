  "logic": "def mask_connection_password(conn_str: str) -> str:\n    \"\"\"Mask the password in a connection string for logging.\"\"\"\n    import re\n    return re.sub(r'(://[^:]+:)[^@]+(@)', r'\\1***\\2', conn_str)",
