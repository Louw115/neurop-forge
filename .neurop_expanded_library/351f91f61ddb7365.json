  "logic": "def build_timing_header(server_time_ms: int, db_time_ms: int, cache_time_ms: int) -> str:\n    \"\"\"Build Server-Timing header value.\"\"\"\n    parts = []\n    if server_time_ms >= 0:\n        parts.append(f'total;dur={server_time_ms}')\n    if db_time_ms >= 0:\n        parts.append(f'db;dur={db_time_ms}')\n    if cache_time_ms >= 0:\n        parts.append(f'cache;dur={cache_time_ms}')\n    return \", \".join(parts)",
