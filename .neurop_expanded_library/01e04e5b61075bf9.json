  "logic": "def build_content_type_header(media_type: str, charset: str, params: dict) -> str:\n    \"\"\"Build a Content-Type header value.\"\"\"\n    value = media_type\n    if charset:\n        value += f\"; charset={charset}\"\n    for key, val in params.items():\n        value += f\"; {key}={val}\"\n    return value",
