  "logic": "def generate_slug(title: str) -> str:\n    \"\"\"Generate URL slug from title.\"\"\"\n    import re\n    slug = title.lower()\n    slug = re.sub(r'[^\\w\\s-]', '', slug)\n    slug = re.sub(r'[\\s_]+', '-', slug)\n    slug = re.sub(r'-+', '-', slug)\n    return slug.strip('-')",
