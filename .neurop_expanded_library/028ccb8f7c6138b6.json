  "logic": "def dedent(v0: str) -> str:\n    v1 = v0.split('\\n')\n    v2 = [v3 for v3 in v1 if v3.strip()]\n    if not v2:\n        return v0\n    v4 = min((len(v3) - len(v3.lstrip()) for v3 in v2))\n    return '\\n'.join((v3[v4:] if len(v3) >= v4 else v3 for v3 in v1))",
