  "logic": "def leak_bucket(v0: dict, v1: int) -> dict:\n    v2 = v1 - v0['last_leak']\n    v3 = v2 // v0['leak_interval_ms']\n    v4 = max(0, v0['level'] - v3 * v0['leak_rate'])\n    return {**v0, 'level': v4, 'last_leak': v1}",
