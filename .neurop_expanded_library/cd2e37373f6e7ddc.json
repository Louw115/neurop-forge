  "logic": "def validate_hostname(hostname: str) -> bool:\n    \"\"\"Validate hostname.\"\"\"\n    import re\n    if len(hostname) > 253:\n        return False\n    pattern = r'^([a-zA-Z0-9]([a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?\\.)*[a-zA-Z]{2,}$'\n    return bool(re.match(pattern, hostname))",
