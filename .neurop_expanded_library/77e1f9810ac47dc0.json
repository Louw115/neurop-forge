  "logic": "def slugify_preserve_unicode(v0: str) -> str:\n    v0 = v0.lower()\n    v0 = v1.sub('[^\\\\w\\\\s-]', '', v0, flags=v1.UNICODE)\n    v0 = v1.sub('[\\\\s_]+', '-', v0)\n    v0 = v1.sub('-+', '-', v0)\n    return v0.strip('-')",
