  "logic": "def fold_tree(node: dict, combine, initial):\n    \"\"\"Fold tree to single value.\"\"\"\n    value = combine(initial, node[\"value\"])\n    for child in node.get(\"children\", []):\n        value = fold_tree(child, combine, value)\n    return value",
