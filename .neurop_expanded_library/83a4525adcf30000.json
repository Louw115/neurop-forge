  "logic": "def get_word_frequency(text: str) -> dict:\n    \"\"\"Get word frequency distribution.\"\"\"\n    words = re.findall(r'\\b\\w+\\b', text.lower())\n    freq = {}\n    for word in words:\n        freq[word] = freq.get(word, 0) + 1\n    return freq",
