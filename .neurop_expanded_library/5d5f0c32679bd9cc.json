  "logic": "def pluralize_word(v0: str, v1: int) -> str:\n    if v1 == 1:\n        return v0\n    if v0.endswith('y') and len(v0) > 1 and (v0[-2] not in 'aeiou'):\n        return v0[:-1] + 'ies'\n    if v0.endswith(('s', 'x', 'z', 'ch', 'sh')):\n        return v0 + 'es'\n    return v0 + 's'",
