  "logic": "def get_max_value(sql_type: str) -> int:\n    \"\"\"Get the maximum value for an integer SQL type.\"\"\"\n    max_values = {\n        \"TINYINT\": 127,\n        \"SMALLINT\": 32767,\n        \"SMALLSERIAL\": 32767,\n        \"INTEGER\": 2147483647,\n        \"INT\": 2147483647,\n        \"SERIAL\": 2147483647,\n        \"BIGINT\": 9223372036854775807,\n        \"BIGSERIAL\": 9223372036854775807,\n    }\n    return max_values.get(sql_type.upper(), 0)",
