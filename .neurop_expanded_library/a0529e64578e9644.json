  "logic": "def get_severity(error_code: str) -> str:\n    \"\"\"Get severity level for error code.\"\"\"\n    category = get_error_category(error_code)\n    if category in [\"SERVER\", \"TIMEOUT\"]:\n        return \"error\"\n    if category in [\"AUTH\", \"PERM\"]:\n        return \"warning\"\n    return \"info\"",
