  "logic": "def parse_currency(amount_str: str) -> float:\n    \"\"\"Parse a currency string to float.\"\"\"\n    cleaned = amount_str.replace(',', '').replace('$', '').replace('\u20ac', '').replace('\u00a3', '').replace('\u00a5', '').strip()\n    try:\n        return float(cleaned)\n    except ValueError:\n        return 0.0",
