  "logic": "def parse_expiry(v0: str) -> dict:\n    v1 = v2.match('^(\\\\d{2})[/\\\\-](\\\\d{2,4})$', v0)\n    if not v1:\n        return {'valid': False, 'month': 0, 'year': 0}\n    v3 = int(v1.group(1))\n    v4 = int(v1.group(2))\n    if v4 < 100:\n        v4 += 2000\n    return {'valid': True, 'month': v3, 'year': v4}",
