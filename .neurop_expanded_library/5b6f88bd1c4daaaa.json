  "logic": "def is_safe_extension(v0: str) -> bool:\n    v0 = v0.lower().lstrip('.')\n    v1 = {'exe', 'msi', 'bat', 'cmd', 'com', 'scr', 'pif', 'vbs', 'vbe', 'js', 'jse', 'ws', 'wsf', 'wsc', 'wsh', 'ps1', 'psm1', 'psd1', 'sh', 'bash', 'zsh', 'csh', 'ksh', 'app', 'dmg', 'pkg', 'deb', 'rpm', 'apk', 'jar', 'war', 'dll', 'so', 'dylib'}\n    return v0 not in v1",
