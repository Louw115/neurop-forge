  "logic": "def lexical_diversity(text: str) -> float:\n    \"\"\"Calculate lexical diversity (unique words / total words).\"\"\"\n    if not text:\n        return 0.0\n    words = text.lower().split()\n    if not words:\n        return 0.0\n    unique = len(set(words))\n    return unique / len(words)",
