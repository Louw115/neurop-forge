  "logic": "def strip_dangerous_attributes(html: str) -> str:\n    \"\"\"Remove dangerous HTML attributes like onclick, onerror, etc.\"\"\"\n    if not html:\n        return \"\"\n    dangerous = [\n        r'\\s+on\\w+\\s*=\\s*[\"\\'][^\"\\']*[\"\\']',\n        r'\\s+on\\w+\\s*=\\s*[^\\s>]+',\n    ]\n    result = html\n    for pattern in dangerous:\n        result = re.sub(pattern, '', result, flags=re.IGNORECASE)\n    return result",
