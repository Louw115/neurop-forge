  "logic": "def format_relative_date(v0: int) -> str:\n    if v0 == 0:\n        return 'today'\n    elif v0 == 1:\n        return 'yesterday'\n    elif v0 == -1:\n        return 'tomorrow'\n    elif v0 > 0:\n        if v0 < 7:\n            return f'{v0} days ago'\n        elif v0 < 30:\n            v1 = v0 // 7\n            return f\"{v1} week{('s' if v1 > 1 else '')} ago\"\n        elif v0 < 365:\n            v2 = v0 // 30\n            return f\"{v2} month{('s' if v2 > 1 else '')} ago\"\n        else:\n            v3 = v0 // 365\n            return f\"{v3} year{('s' if v3 > 1 else '')} ago\"\n    else:\n        v4 = abs(v0)\n        if v4 < 7:\n            return f'in {v4} days'\n        elif v4 < 30:\n            v1 = v4 // 7\n            return f\"in {v1} week{('s' if v1 > 1 else '')}\"\n        else:\n            v2 = v4 // 30\n            return f\"in {v2} month{('s' if v2 > 1 else '')}\"",
