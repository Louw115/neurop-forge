  "logic": "def is_phone_format(value: str) -> dict:\n    \"\"\"Check phone number format.\"\"\"\n    pattern = r'^[\\d\\s\\-\\+\\(\\)]{7,20}$'\n    valid = bool(re.match(pattern, value))\n    return {\"valid\": valid, \"error\": \"Invalid phone format\" if not valid else None}",
