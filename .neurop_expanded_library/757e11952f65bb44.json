  "logic": "def constant_time_compare(a: str, b: str) -> bool:\n    \"\"\"Compare two strings in constant time.\"\"\"\n    if len(a) != len(b):\n        return False\n    result = 0\n    for x, y in zip(a.encode(), b.encode()):\n        result |= x ^ y\n    return result == 0",
