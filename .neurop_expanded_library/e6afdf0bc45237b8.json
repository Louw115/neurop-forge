  "logic": "def lru_touch(v0: dict, v1: str) -> dict:\n    if v1 not in v0['items']:\n        return v0\n    v2 = [v3 for v3 in v0['order'] if v3 != v1]\n    v2.append(v1)\n    return {'capacity': v0['capacity'], 'items': dict(v0['items']), 'order': v2, 'size': v0['size']}",
