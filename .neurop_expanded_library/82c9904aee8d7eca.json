  "logic": "def refill_tokens(v0: dict, v1: float) -> dict:\n    v2 = v1 - v0['last_refill']\n    v3 = min(v0['capacity'], v0['tokens'] + v2 * v0['refill_rate'])\n    return {'capacity': v0['capacity'], 'refill_rate': v0['refill_rate'], 'tokens': v3, 'last_refill': v1}",
