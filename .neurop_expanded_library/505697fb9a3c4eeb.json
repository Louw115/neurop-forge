  "logic": "def is_valid_phone(v0: str) -> bool:\n    if not v0:\n        return False\n    v1 = v2.sub('[\\\\s\\\\-\\\\.\\\\(\\\\)]', '', v0)\n    v3 = '^\\\\+?[0-9]{10,15}$'\n    return bool(v2.match(v3, v1))",
