  "logic": "def minify_html(v0: str) -> str:\n    v0 = v1.sub('<!--.*?-->', '', v0, flags=v1.DOTALL)\n    v0 = v1.sub('>\\\\s+<', '><', v0)\n    v0 = v1.sub('\\\\s+', ' ', v0)\n    return v0.strip()",
