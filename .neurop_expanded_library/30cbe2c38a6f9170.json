  "logic": "def shuffle_deterministic(arr: list, seed: int) -> list:\n    \"\"\"Deterministic shuffle using seed.\"\"\"\n    import hashlib\n    result = list(arr)\n    n = len(result)\n    for i in range(n - 1, 0, -1):\n        h = hashlib.sha256(f\"{seed}-{i}\".encode()).digest()\n        j = h[0] % (i + 1)\n        result[i], result[j] = result[j], result[i]\n    return result",
