  "logic": "def parse_cidr(cidr: str) -> dict:\n    \"\"\"Parse CIDR notation.\"\"\"\n    if \"/\" not in cidr:\n        return {\"network\": cidr, \"prefix\": 32, \"valid\": validate_ipv4(cidr)}\n    parts = cidr.split(\"/\")\n    network = parts[0]\n    prefix = int(parts[1])\n    return {\n        \"network\": network,\n        \"prefix\": prefix,\n        \"valid\": validate_ipv4(network) and 0 <= prefix <= 32\n    }",
