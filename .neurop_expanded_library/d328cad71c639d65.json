  "logic": "def validate_accept_header(v0: str, v1: list) -> dict:\n    if not v0 or v0 == '*/*':\n        return {'valid': True, 'error': None}\n    v2 = [v3.strip().lower() for v3 in v0.split(',')]\n    for v4 in v1:\n        if v4.lower() in v2:\n            return {'valid': True, 'error': None}\n    return {'valid': False, 'error': f\"Cannot satisfy Accept header. Supported: {', '.join(v1)}\"}",
