  "logic": "def assert_less_than(value, threshold, message: str) -> dict:\n    \"\"\"Assert value is less than threshold.\"\"\"\n    passed = value < threshold\n    return {\n        \"passed\": passed,\n        \"message\": message,\n        \"actual\": str(value),\n        \"expected\": f\"< {threshold}\",\n        \"error\": None if passed else f\"{value} is not less than {threshold}\"\n    }",
