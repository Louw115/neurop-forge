  "logic": "def compare_checksums(checksum1: str, checksum2: str) -> bool:\n    \"\"\"Constant-time comparison of checksums.\"\"\"\n    if len(checksum1) != len(checksum2):\n        return False\n    result = 0\n    for a, b in zip(checksum1.lower(), checksum2.lower()):\n        result |= ord(a) ^ ord(b)\n    return result == 0",
