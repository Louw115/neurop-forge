  "logic": "def distance_2d(x1: float, y1: float, x2: float, y2: float) -> float:\n    \"\"\"Calculate the Euclidean distance between two 2D points.\"\"\"\n    return math.sqrt((x2 - x1) ** 2 + (y2 - y1) ** 2)",
