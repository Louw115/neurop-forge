  "logic": "def parse_data_uri(v0: str) -> dict:\n    v1 = v2.match('^data:([^;,]*)?(?:;(base64))?,(.*)$', v0)\n    if not v1:\n        return None\n    v3, v4, v5 = v1.groups()\n    return {'mime_type': v3 or 'text/plain', 'encoding': v4, 'data': v5}",
