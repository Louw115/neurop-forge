  "logic": "def bst_delete(v0: dict, v1) -> dict:\n\n    def min_node(v2):\n        while v2['left']:\n            v2 = v2['left']\n        return v2\n\n    def delete_node(v2, v1):\n        if v2 is None:\n            return None\n        if v1 < v2['key']:\n            return v3(v2['key'], v2['value'], v4(v2['left'], v1), v2['right'])\n        elif v1 > v2['key']:\n            return v3(v2['key'], v2['value'], v2['left'], v4(v2['right'], v1))\n        else:\n            if v2['left'] is None:\n                return v2['right']\n            if v2['right'] is None:\n                return v2['left']\n            v5 = v6(v2['right'])\n            return v3(v5['key'], v5['value'], v2['left'], v4(v2['right'], v5['key']))\n    if not v7(v0, v1):\n        return v0\n    return {'root': v4(v0['root'], v1), 'size': v0['size'] - 1}",
