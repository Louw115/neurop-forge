  "logic": "def time_until_tokens(v0: dict, v1: int, v2: float) -> float:\n    v3 = v4(v0, v2)\n    if v3['tokens'] >= v1:\n        return 0\n    v5 = v1 - v3['tokens']\n    return v5 / v3['refill_rate']",
