  "logic": "def is_code_file(filename: str) -> bool:\n    \"\"\"Check if file is a code file.\"\"\"\n    ext = get_file_extension(filename)\n    return ext in {\"py\", \"js\", \"ts\", \"jsx\", \"tsx\", \"java\", \"c\", \"cpp\", \"h\", \"cs\", \"go\", \"rs\", \"rb\", \"php\"}",
