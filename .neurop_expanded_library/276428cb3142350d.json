  "logic": "def mask_phone(phone: str) -> str:\n    \"\"\"Mask phone number showing only last 4 digits.\"\"\"\n    digits = re.sub(r'\\D', '', phone)\n    if len(digits) < 4:\n        return \"*\" * len(phone)\n    masked = \"*\" * (len(digits) - 4) + digits[-4:]\n    return masked",
