  "logic": "def extract_dates(text: str) -> list:\n    \"\"\"Extract date patterns from text.\"\"\"\n    patterns = [\n        r'\\d{4}-\\d{2}-\\d{2}',\n        r'\\d{1,2}/\\d{1,2}/\\d{2,4}',\n        r'\\d{1,2}-\\d{1,2}-\\d{2,4}'\n    ]\n    dates = []\n    for pattern in patterns:\n        dates.extend(re.findall(pattern, text))\n    return dates",
