  "logic": "def trie_search(v0: dict, v1: str) -> dict:\n    v2 = v0['root']\n    for v3 in v1:\n        if v3 not in v2['children']:\n            return {'found': False, 'value': None}\n        v2 = v2['children'][v3]\n    if v2['is_end']:\n        return {'found': True, 'value': v2['value']}\n    return {'found': False, 'value': None}",
