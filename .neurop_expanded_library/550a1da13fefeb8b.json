  "logic": "def build_mailto_link(email: str, subject: str, body: str) -> str:\n    \"\"\"Build a mailto: link.\"\"\"\n    from urllib.parse import quote\n    link = f\"mailto:{email}\"\n    params = []\n    if subject:\n        params.append(f\"subject={quote(subject)}\")\n    if body:\n        params.append(f\"body={quote(body)}\")\n    if params:\n        link += '?' + '&'.join(params)\n    return link",
