  "logic": "def fixed_window_allow(v0: dict, v1: float) -> dict:\n    v2 = v0['window_start']\n    v3 = v0['count']\n    if v1 >= v2 + v0['window_size']:\n        v2 = v1 - v1 % v0['window_size']\n        v3 = 0\n    if v3 < v0['max_requests']:\n        return {'window': {**v0, 'window_start': v2, 'count': v3 + 1}, 'allowed': True, 'remaining': v0['max_requests'] - v3 - 1}\n    return {'window': {**v0, 'window_start': v2, 'count': v3}, 'allowed': False, 'remaining': 0}",
