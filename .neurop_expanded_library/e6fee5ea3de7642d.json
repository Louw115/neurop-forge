  "logic": "def mask_contact_email(v0: dict) -> dict:\n    v1 = v0.get('email', '')\n    if '@' in v1:\n        v2, v3 = v1.rsplit('@', 1)\n        v4 = v2[0] + '***' + '@' + v3\n    else:\n        v4 = v1\n    return {**v0, 'email': v4}",
