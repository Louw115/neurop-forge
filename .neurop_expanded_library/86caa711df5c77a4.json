  "logic": "def exists(obj, path: str) -> bool:\n    \"\"\"Check if path exists.\"\"\"\n    parts = parse_path(path)\n    current = obj\n    for part in parts:\n        if isinstance(current, dict):\n            if part not in current:\n                return False\n            current = current[part]\n        elif isinstance(current, list):\n            try:\n                idx = int(part)\n                if 0 <= idx < len(current):\n                    current = current[idx]\n                else:\n                    return False\n            except ValueError:\n                return False\n        else:\n            return False\n    return True",
