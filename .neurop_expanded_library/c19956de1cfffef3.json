  "logic": "def build_connection_string(protocol: str, host: str, port: int, path: str, auth: str) -> str:\n    \"\"\"Build a connection string from components.\"\"\"\n    result = f\"{protocol}://\"\n    if auth:\n        result += f\"{auth}@\"\n    result += host\n    if port:\n        result += f\":{port}\"\n    if path:\n        result += f\"/{path}\"\n    return result",
