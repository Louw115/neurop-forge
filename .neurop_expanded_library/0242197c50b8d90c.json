  "logic": "def slugify(v0: str) -> str:\n    v0 = v0.lower()\n    v0 = v1.sub('[^a-z0-9\\\\s\\\\-]', '', v0)\n    v0 = v1.sub('[\\\\s_]+', '-', v0)\n    v0 = v1.sub('-+', '-', v0)\n    return v0.strip('-')",
