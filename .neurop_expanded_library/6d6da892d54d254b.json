  "logic": "def fletcher16(data: bytes) -> int:\n    \"\"\"Calculate Fletcher-16 checksum.\"\"\"\n    sum1 = 0\n    sum2 = 0\n    for byte in data:\n        sum1 = (sum1 + byte) % 255\n        sum2 = (sum2 + sum1) % 255\n    return (sum2 << 8) | sum1",
