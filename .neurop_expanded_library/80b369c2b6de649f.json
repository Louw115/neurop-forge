  "logic": "def get_filename(path: str) -> str:\n    \"\"\"Extract the filename from a path.\"\"\"\n    if not path:\n        return \"\"\n    path = path.replace('\\\\', '/')\n    if '/' in path:\n        return path.rsplit('/', 1)[-1]\n    return path",
