  "logic": "def create_element(tag: str, content: str, attrs: dict) -> str:\n    \"\"\"Create HTML element.\"\"\"\n    if attrs:\n        attr_str = \" \".join(f'{k}=\"{escape_html(str(v))}\"' for k, v in attrs.items())\n        return f\"<{tag} {attr_str}>{content}</{tag}>\"\n    return f\"<{tag}>{content}</{tag}>\"",
