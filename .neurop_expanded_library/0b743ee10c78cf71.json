  "logic": "def unescape_html(text: str) -> str:\n    \"\"\"Unescape HTML entities.\"\"\"\n    return (text\n        .replace(\"&amp;\", \"&\")\n        .replace(\"&lt;\", \"<\")\n        .replace(\"&gt;\", \">\")\n        .replace(\"&quot;\", '\"')\n        .replace(\"&#39;\", \"'\"))",
