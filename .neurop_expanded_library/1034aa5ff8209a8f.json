  "logic": "def bound_check(value: float, min_val: float, max_val: float) -> dict:\n    \"\"\"Check if value is within bounds.\"\"\"\n    return {\n        \"in_bounds\": min_val <= value <= max_val,\n        \"value\": value,\n        \"min\": min_val,\n        \"max\": max_val,\n        \"below\": value < min_val,\n        \"above\": value > max_val\n    }",
