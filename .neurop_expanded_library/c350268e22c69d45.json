  "logic": "def most_common_word(text: str) -> str:\n    \"\"\"Find the most common word in a text.\"\"\"\n    freq = word_frequency(text)\n    if not freq:\n        return \"\"\n    return max(freq.keys(), key=lambda k: freq[k])",
