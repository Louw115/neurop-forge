  "logic": "def evaluate_postfix(tokens: list, variables: dict) -> float:\n    \"\"\"Evaluate postfix expression.\"\"\"\n    stack = []\n    for token in tokens:\n        if token[\"type\"] == \"number\":\n            stack.append(token[\"value\"])\n        elif token[\"type\"] == \"identifier\":\n            stack.append(variables.get(token[\"value\"], 0))\n        elif token[\"type\"] == \"operator\":\n            if len(stack) < 2:\n                return 0\n            b = stack.pop()\n            a = stack.pop()\n            op = token[\"value\"]\n            if op == \"+\":\n                stack.append(a + b)\n            elif op == \"-\":\n                stack.append(a - b)\n            elif op == \"*\":\n                stack.append(a * b)\n            elif op == \"/\":\n                stack.append(a / b if b != 0 else 0)\n            elif op == \"%\":\n                stack.append(a % b if b != 0 else 0)\n            elif op == \"^\":\n                stack.append(a ** b)\n    return stack[0] if stack else 0",
