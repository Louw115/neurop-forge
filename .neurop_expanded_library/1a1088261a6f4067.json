  "logic": "def pluralize(v0: str, v1: int) -> str:\n    if v1 == 1:\n        return v0\n    if v0.endswith('y') and (not v0.endswith(('ay', 'ey', 'iy', 'oy', 'uy'))):\n        return v0[:-1] + 'ies'\n    if v0.endswith(('s', 'sh', 'ch', 'x', 'z')):\n        return v0 + 'es'\n    return v0 + 's'",
