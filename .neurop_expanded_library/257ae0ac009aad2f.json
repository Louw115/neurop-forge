  "logic": "def truth_table_row(inputs: list, operation: str) -> dict:\n    \"\"\"Generate truth table row for operation.\"\"\"\n    if operation == \"and\":\n        result = all(inputs)\n    elif operation == \"or\":\n        result = any(inputs)\n    elif operation == \"xor\":\n        result = sum(inputs) % 2 == 1\n    else:\n        result = False\n    return {\"inputs\": inputs, \"result\": result}",
