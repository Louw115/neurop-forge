  "logic": "def get_env_tier(env_name: str) -> str:\n    \"\"\"Get the tier classification of an environment.\"\"\"\n    name = env_name.lower()\n    if name in ('prod', 'production', 'live'):\n        return 'production'\n    if name in ('stage', 'staging', 'uat', 'preprod'):\n        return 'staging'\n    if name in ('test', 'testing', 'ci'):\n        return 'testing'\n    return 'development'",
