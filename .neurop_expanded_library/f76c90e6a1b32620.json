  "logic": "def is_safe_identifier(v0: str) -> bool:\n    import re\n    if not v0 or not v1.match('^[a-zA-Z_][a-zA-Z0-9_]*$', v0):\n        return False\n    v2 = {'SELECT', 'FROM', 'WHERE', 'INSERT', 'UPDATE', 'DELETE', 'TABLE', 'INDEX', 'CREATE', 'DROP'}\n    return v0.upper() not in v2",
