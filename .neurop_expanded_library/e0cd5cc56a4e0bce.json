  "logic": "def mask_string(text: str, start: int, end: int, mask_char: str) -> str:\n    \"\"\"Mask portion of string.\"\"\"\n    if start >= len(text):\n        return text\n    end = min(end, len(text))\n    return text[:start] + mask_char * (end - start) + text[end:]",
