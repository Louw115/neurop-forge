  "logic": "def add_years(year: int, month: int, day: int, years_to_add: int) -> dict:\n    \"\"\"Add years to a date.\"\"\"\n    new_year = year + years_to_add\n    new_day = min(day, days_in_month(new_year, month))\n    return {\"year\": new_year, \"month\": month, \"day\": new_day}",
