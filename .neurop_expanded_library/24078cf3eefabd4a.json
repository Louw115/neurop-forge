  "logic": "def add_filter_to_query(query: dict, field: str, operator: str, value) -> dict:\n    \"\"\"Add a filter to search query.\"\"\"\n    result = dict(query)\n    result[\"filters\"] = list(query.get(\"filters\", []))\n    result[\"filters\"].append({\n        \"field\": field,\n        \"operator\": operator,\n        \"value\": value\n    })\n    return result",
