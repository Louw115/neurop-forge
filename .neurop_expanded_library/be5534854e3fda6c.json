  "logic": "def summarize_assertions(results: list) -> dict:\n    \"\"\"Summarize assertion results.\"\"\"\n    total = len(results)\n    passed = sum(1 for r in results if r.get(\"passed\"))\n    failed = total - passed\n    return {\n        \"total\": total,\n        \"passed\": passed,\n        \"failed\": failed,\n        \"pass_rate\": passed / total if total > 0 else 0,\n        \"all_passed\": failed == 0\n    }",
