  "logic": "def reverse_vat(gross_amount: float, vat_rate: float) -> dict:\n    \"\"\"Reverse calculate VAT from gross.\"\"\"\n    net = round(gross_amount / (1 + vat_rate / 100), 2)\n    vat = round(gross_amount - net, 2)\n    return {\n        \"net\": net,\n        \"vat\": vat,\n        \"gross\": gross_amount\n    }",
