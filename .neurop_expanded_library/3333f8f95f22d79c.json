  "logic": "def convert_links_to_references(v0: str) -> str:\n    v1 = v2(v0)\n    v3 = v0\n    v4 = []\n    for v5, v6 in enumerate(v1, 1):\n        v7 = str(v5)\n        v8 = f\"[{v6['text']}]({v6['url']})\"\n        v9 = f\"[{v6['text']}][{v7}]\"\n        v3 = v3.replace(v8, v9, 1)\n        v4.append(f\"[{v7}]: {v6['url']}\")\n    if v4:\n        v3 += '\\n\\n' + '\\n'.join(v4)\n    return v3",
