  "logic": "def parse_version(v0: str) -> dict:\n    v1 = '^v?(\\\\d+)\\\\.(\\\\d+)\\\\.(\\\\d+)(?:-([a-zA-Z0-9.-]+))?(?:\\\\+([a-zA-Z0-9.-]+))?$'\n    v2 = v3.match(v1, v0)\n    if not v2:\n        return None\n    return {'major': int(v2.group(1)), 'minor': int(v2.group(2)), 'patch': int(v2.group(3)), 'prerelease': v2.group(4) or '', 'build': v2.group(5) or ''}",
