  "logic": "def extended_gcd(v0: int, v1: int) -> dict:\n    if v1 == 0:\n        return {'gcd': v0, 'x': 1, 'y': 0}\n    v2 = v3(v1, v0 % v1)\n    return {'gcd': v2['gcd'], 'x': v2['y'], 'y': v2['x'] - v0 // v1 * v2['y']}",
