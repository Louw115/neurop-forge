  "logic": "def parse_connection_string(v0: str) -> dict:\n    import re\n    v1 = {'host': '', 'port': 5432, 'database': '', 'user': '', 'password': '', 'options': {}}\n    v2 = '^(?:(\\\\w+)://)?(?:([^:@]+)(?::([^@]*))?@)?([^:/]+)?(?::(\\\\d+))?(?:/([^?]+))?(?:\\\\?(.+))?$'\n    v3 = v4.match(v2, v0)\n    if v3:\n        if v3.group(2):\n            v1['user'] = v3.group(2)\n        if v3.group(3):\n            v1['password'] = v3.group(3)\n        if v3.group(4):\n            v1['host'] = v3.group(4)\n        if v3.group(5):\n            v1['port'] = int(v3.group(5))\n        if v3.group(6):\n            v1['database'] = v3.group(6)\n        if v3.group(7):\n            for v5 in v3.group(7).split('&'):\n                if '=' in v5:\n                    v6, v7 = v5.split('=', 1)\n                    v1['options'][v6] = v7\n    return v1",
