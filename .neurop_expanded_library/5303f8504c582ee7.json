  "logic": "def get_char_name(v0: str) -> str:\n    v1 = v2(v0)\n    v3 = {0: 'NUL', 7: 'BEL', 8: 'BS', 9: 'TAB', 10: 'LF', 13: 'CR', 27: 'ESC', 32: 'SPACE', 127: 'DEL'}\n    if v1 in v3:\n        return v3[v1]\n    if v4(v0):\n        return v0\n    return f'0x{v1:02X}'",
