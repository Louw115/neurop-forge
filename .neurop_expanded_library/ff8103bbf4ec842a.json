  "logic": "def parse_cookie_header(cookie_header: str) -> dict:\n    \"\"\"Parse a Cookie header into a dictionary.\"\"\"\n    cookies = {}\n    if not cookie_header:\n        return cookies\n    for pair in cookie_header.split(';'):\n        pair = pair.strip()\n        if '=' in pair:\n            key, value = pair.split('=', 1)\n            cookies[key.strip()] = value.strip()\n    return cookies",
