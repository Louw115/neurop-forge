  "logic": "def parse_cron_expression(cron: str) -> dict:\n    \"\"\"Parse a cron expression into components.\"\"\"\n    parts = cron.split()\n    if len(parts) != 5:\n        return {\"valid\": False, \"error\": \"Invalid cron format\"}\n    return {\n        \"valid\": True,\n        \"minute\": parts[0],\n        \"hour\": parts[1],\n        \"day_of_month\": parts[2],\n        \"month\": parts[3],\n        \"day_of_week\": parts[4]\n    }",
