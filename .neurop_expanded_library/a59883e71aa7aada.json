  "logic": "def random_poisson(seed: int, lam: float) -> int:\n    \"\"\"Generate deterministic Poisson random number.\"\"\"\n    import math\n    L = math.exp(-lam)\n    k = 0\n    p = 1.0\n    while p > L:\n        k += 1\n        p *= random_float(hash_seed(seed, f\"poisson_{k}\"))\n    return k - 1",
