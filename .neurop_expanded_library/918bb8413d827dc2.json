  "logic": "def cross_product_3d(a: list, b: list) -> list:\n    \"\"\"Calculate the cross product of two 3D vectors.\"\"\"\n    if len(a) != 3 or len(b) != 3:\n        return []\n    return [\n        a[1] * b[2] - a[2] * b[1],\n        a[2] * b[0] - a[0] * b[2],\n        a[0] * b[1] - a[1] * b[0],\n    ]",
