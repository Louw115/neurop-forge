  "logic": "def parse_us_address(v0: str) -> dict:\n    v1 = v2(v0)\n    v3 = {'street1': '', 'street2': '', 'city': '', 'state': '', 'zip': ''}\n    if v1:\n        v3['street1'] = v1[0]\n    if len(v1) > 1:\n        v4 = v1[-1]\n        v5 = v6.match('^(.+?),?\\\\s+([A-Z]{2})\\\\s+(\\\\d{5}(?:-\\\\d{4})?)$', v4, v6.IGNORECASE)\n        if v5:\n            v3['city'] = v5.group(1).strip(',')\n            v3['state'] = v5.group(2).upper()\n            v3['zip'] = v5.group(3)\n        if len(v1) > 2:\n            v3['street2'] = v1[1]\n    return v3",
