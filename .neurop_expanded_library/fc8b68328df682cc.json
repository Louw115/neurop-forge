  "logic": "def increment_slug_number(v0: str) -> str:\n    v1 = v2.search('-(\\\\d+)$', v0)\n    if v1:\n        v3 = int(v1.group(1)) + 1\n        return v0[:v1.start()] + f'-{v3}'\n    return f'{v0}-1'",
