  },
  "logic": "def interpolate_missing(v0: list) -> list:\n    if not v0:\n        return []\n    v1 = v0.copy()\n    for v2 in range(len(v1)):\n        if v1[v2] is None:\n            v3 = None\n            v4 = None\n            for v5 in range(v2 - 1, -1, -1):\n                if v1[v5] is not None:\n                    v3 = v5\n                    break\n            for v5 in range(v2 + 1, len(v1)):\n                if v1[v5] is not None:\n                    v4 = v5\n                    break\n            if v3 is not None and v4 is not None:\n                v6 = (v2 - v3) / (v4 - v3)\n                v1[v2] = v1[v3] + (v1[v4] - v1[v3]) * v6\n            elif v3 is not None:\n                v1[v2] = v1[v3]\n            elif v4 is not None:\n                v1[v2] = v1[v4]\n    return v1",
