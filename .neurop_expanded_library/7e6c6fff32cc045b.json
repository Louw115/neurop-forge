  "logic": "def calculate_upload_progress(bytes_uploaded: int, total_bytes: int) -> float:\n    \"\"\"Calculate upload progress percentage.\"\"\"\n    if total_bytes <= 0:\n        return 0.0\n    return min(100.0, (bytes_uploaded / total_bytes) * 100)",
