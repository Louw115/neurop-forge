  "logic": "def slugify(text: str) -> str:\n    \"\"\"Convert text to URL slug.\"\"\"\n    text = unicodedata.normalize(\"NFKD\", text)\n    text = text.encode(\"ascii\", \"ignore\").decode(\"ascii\")\n    text = text.lower()\n    text = re.sub(r\"[^a-z0-9\\s-]\", \"\", text)\n    text = re.sub(r\"[\\s_]+\", \"-\", text)\n    text = re.sub(r\"-+\", \"-\", text)\n    return text.strip(\"-\")",
