  "logic": "def point_scale(point: dict, origin: dict, factor: float) -> dict:\n    \"\"\"Scale point relative to origin.\"\"\"\n    new_x = origin[\"x\"] + (point[\"x\"] - origin[\"x\"]) * factor\n    new_y = origin[\"y\"] + (point[\"y\"] - origin[\"y\"]) * factor\n    return create_point(new_x, new_y)",
