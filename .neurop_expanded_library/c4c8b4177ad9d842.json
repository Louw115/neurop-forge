  "logic": "def complement_color(r: int, g: int, b: int) -> dict:\n    \"\"\"Get complementary color.\"\"\"\n    hsl = rgb_to_hsl(r, g, b)\n    new_h = (hsl[\"h\"] + 180) % 360\n    return hsl_to_rgb(new_h, hsl[\"s\"], hsl[\"l\"])",
