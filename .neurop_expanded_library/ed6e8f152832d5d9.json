  "logic": "def count_special(password: str) -> int:\n    \"\"\"Count special characters.\"\"\"\n    special = set(\"!@#$%^&*()_+-=[]{}|;':\\\",./<>?`~\")\n    return sum(1 for c in password if c in special)",
