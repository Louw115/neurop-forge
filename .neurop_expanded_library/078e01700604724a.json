  "logic": "def diff_similarity(diff: list) -> float:\n    \"\"\"Calculate similarity from diff.\"\"\"\n    counts = count_diff_changes(diff)\n    total = counts[\"insertions\"] + counts[\"deletions\"] + counts[\"unchanged\"]\n    if total == 0:\n        return 1.0\n    return counts[\"unchanged\"] / total",
