  "logic": "def extract_text(v0: str) -> str:\n    v1 = v2.sub('<script[^>]*>.*?</script>', '', v0, flags=v2.DOTALL | v2.IGNORECASE)\n    v1 = v2.sub('<style[^>]*>.*?</style>', '', v1, flags=v2.DOTALL | v2.IGNORECASE)\n    v1 = v3(v1)\n    return ' '.join(v1.split())",
