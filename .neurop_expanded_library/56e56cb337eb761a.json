  "logic": "def get_domain(v0: str) -> str:\n    v1 = v2(v0)\n    if not v1:\n        return ''\n    v3 = v1.split('.')\n    if len(v3) >= 2:\n        return '.'.join(v3[-2:])\n    return v1",
