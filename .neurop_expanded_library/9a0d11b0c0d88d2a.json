  "logic": "def is_between(value: float, min_val: float, max_val: float, inclusive: bool) -> bool:\n    \"\"\"Check if value is in range.\"\"\"\n    if inclusive:\n        return min_val <= value <= max_val\n    return min_val < value < max_val",
