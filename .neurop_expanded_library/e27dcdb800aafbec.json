  "logic": "def generate_test_name(v0: str, v1: int) -> str:\n    v2 = ['John', 'Jane', 'Bob', 'Alice', 'Charlie', 'Diana', 'Eve', 'Frank']\n    v3 = ['Smith', 'Johnson', 'Williams', 'Brown', 'Jones', 'Garcia', 'Miller', 'Davis']\n    import hashlib\n    v4 = v5.sha256(f'name:{v1}'.encode()).digest()\n    v6 = v2[v4[0] % len(v2)]\n    v7 = v3[v4[1] % len(v3)]\n    return f'{v0}{v6} {v7}' if v0 else f'{v6} {v7}'",
