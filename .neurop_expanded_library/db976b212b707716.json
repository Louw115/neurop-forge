  "logic": "def aggregate_validation_errors(errors: list) -> dict:\n    \"\"\"Aggregate validation errors by field.\"\"\"\n    by_field = {}\n    for error in errors:\n        field = error.get(\"field\", \"_general\")\n        if field not in by_field:\n            by_field[field] = []\n        by_field[field].append({\n            \"code\": error.get(\"code\"),\n            \"rule\": error.get(\"rule\"),\n            \"message\": error.get(\"message\")\n        })\n    return by_field",
