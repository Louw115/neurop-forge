  "logic": "def contains_sql_injection_patterns(text: str) -> bool:\n    \"\"\"Check if text contains common SQL injection patterns.\"\"\"\n    if not text:\n        return False\n    patterns = [\n        r\"('\\s*OR\\s+')\",\n        r\"('\\s*AND\\s+')\",\n        r\"(--\\s*$)\",\n        r\"(;\\s*DROP\\s+)\",\n        r\"(;\\s*DELETE\\s+)\",\n        r\"(UNION\\s+SELECT)\",\n        r\"(INSERT\\s+INTO)\",\n        r\"(UPDATE\\s+\\w+\\s+SET)\",\n    ]\n    text_upper = text.upper()\n    for pattern in patterns:\n        if re.search(pattern, text_upper, re.IGNORECASE):\n            return True\n    return False",
