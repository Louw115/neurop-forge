  "logic": "def validate_expiry(month: int, year: int, current_month: int, current_year: int) -> bool:\n    \"\"\"Validate expiry date.\"\"\"\n    if month < 1 or month > 12:\n        return False\n    if year < current_year:\n        return False\n    if year == current_year and month < current_month:\n        return False\n    return True",
