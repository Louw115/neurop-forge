  "logic": "def round_to_minute(time: dict) -> dict:\n    \"\"\"Round time to nearest minute.\"\"\"\n    if time[\"seconds\"] >= 30:\n        return add_seconds(time, 60 - time[\"seconds\"])\n    return {\"hours\": time[\"hours\"], \"minutes\": time[\"minutes\"], \"seconds\": 0}",
