  "logic": "def generate_webhook_signature(payload: str, secret: str, algorithm: str) -> str:\n    \"\"\"Generate HMAC signature for webhook payload.\"\"\"\n    import hmac\n    import hashlib\n    if algorithm == \"sha256\":\n        hash_func = hashlib.sha256\n    elif algorithm == \"sha1\":\n        hash_func = hashlib.sha1\n    else:\n        hash_func = hashlib.sha256\n    signature = hmac.new(\n        secret.encode(),\n        payload.encode(),\n        hash_func\n    ).hexdigest()\n    return signature",
