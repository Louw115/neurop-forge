  "logic": "def format_rating_stars(rating: float, max_rating: int) -> str:\n    \"\"\"Format rating as star string.\"\"\"\n    full = int(rating)\n    half = 1 if rating - full >= 0.5 else 0\n    empty = max_rating - full - half\n    return \"\u2605\" * full + \"\u2606\" * half + \"\u2606\" * empty",
