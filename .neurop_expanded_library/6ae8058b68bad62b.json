  "logic": "def parse_query_string(query: str) -> dict:\n    \"\"\"Parse query string into dictionary.\"\"\"\n    if not query:\n        return {}\n    result = {}\n    for pair in query.split(\"&\"):\n        if \"=\" in pair:\n            key, value = pair.split(\"=\", 1)\n            result[key] = value\n        else:\n            result[pair] = \"\"\n    return result",
