  "logic": "def dms_to_decimal(v0: float, v1: float, v2: float, v3: str) -> float:\n    v4 = abs(v0) + v1 / 60 + v2 / 3600\n    if v3.upper() in ['S', 'W']:\n        v4 = -v4\n    return v4",
