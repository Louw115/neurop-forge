  "logic": "def remove_slug_suffix(slug: str, suffix: str) -> str:\n    \"\"\"Remove a suffix from a slug.\"\"\"\n    if slug.endswith(f\"-{suffix}\"):\n        return slug[:-len(suffix)-1]\n    return slug",
