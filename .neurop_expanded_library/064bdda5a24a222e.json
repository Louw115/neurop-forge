  "logic": "def rectangles_intersect(r1: dict, r2: dict) -> bool:\n    \"\"\"Check if two rectangles intersect.\"\"\"\n    return not (r1[\"x\"] + r1[\"width\"] < r2[\"x\"] or\n                r2[\"x\"] + r2[\"width\"] < r1[\"x\"] or\n                r1[\"y\"] + r1[\"height\"] < r2[\"y\"] or\n                r2[\"y\"] + r2[\"height\"] < r1[\"y\"])",
