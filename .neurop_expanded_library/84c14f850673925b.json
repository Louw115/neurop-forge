  "logic": "def get_country_from_zip(v0: str) -> str:\n    if v1.match('^\\\\d{5}(-\\\\d{4})?$', v0):\n        return 'US'\n    if v1.match('^[A-Z]\\\\d[A-Z]\\\\s?\\\\d[A-Z]\\\\d$', v0, v1.IGNORECASE):\n        return 'CA'\n    if v1.match('^[A-Z]{1,2}\\\\d{1,2}[A-Z]?\\\\s?\\\\d[A-Z]{2}$', v0, v1.IGNORECASE):\n        return 'GB'\n    return ''",
