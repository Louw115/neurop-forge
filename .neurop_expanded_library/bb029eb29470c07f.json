  "logic": "def lru_get_stats(cache: dict) -> dict:\n    \"\"\"Get cache statistics.\"\"\"\n    return {\n        \"size\": cache[\"size\"],\n        \"capacity\": cache[\"capacity\"],\n        \"utilization\": cache[\"size\"] / cache[\"capacity\"] if cache[\"capacity\"] > 0 else 0,\n        \"lru_key\": lru_lru_key(cache),\n        \"mru_key\": lru_mru_key(cache)\n    }",
