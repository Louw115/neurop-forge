  "logic": "def parse_connection_string(conn_str: str) -> dict:\n    \"\"\"Parse a connection string into components.\"\"\"\n    result = {'raw': conn_str}\n    if '://' in conn_str:\n        protocol, rest = conn_str.split('://', 1)\n        result['protocol'] = protocol\n        if '@' in rest:\n            auth, host_part = rest.rsplit('@', 1)\n            result['auth'] = auth\n            rest = host_part\n        if '/' in rest:\n            host_port, path = rest.split('/', 1)\n            result['path'] = path\n        else:\n            host_port = rest\n        if ':' in host_port:\n            host, port = host_port.rsplit(':', 1)\n            result['host'] = host\n            result['port'] = port\n        else:\n            result['host'] = host_port\n    return result",
