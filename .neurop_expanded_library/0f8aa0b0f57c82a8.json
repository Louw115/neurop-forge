  "logic": "def url_encode(v0: str) -> str:\n    if not v0:\n        return ''\n    v1 = []\n    for v2 in v0:\n        if v2.isalnum() or v2 in '-_.~':\n            v1.append(v2)\n        elif v2 == ' ':\n            v1.append('+')\n        else:\n            v3 = v2.encode('utf-8')\n            for v4 in v3:\n                v1.append(f'%{v4:02X}')\n    return ''.join(v1)",
