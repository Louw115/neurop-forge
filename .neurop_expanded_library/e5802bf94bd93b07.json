  "logic": "def is_balanced_parentheses(s: str) -> bool:\n    \"\"\"Check if parentheses are balanced using stack logic.\"\"\"\n    stack = []\n    pairs = {\")\": \"(\", \"}\": \"{\", \"]\": \"[\"}\n    for char in s:\n        if char in \"({[\":\n            stack.append(char)\n        elif char in \")}]\":\n            if not stack or stack[-1] != pairs[char]:\n                return False\n            stack.pop()\n    return len(stack) == 0",
