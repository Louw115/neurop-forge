  "logic": "def build_service_manifest(name: str, port: int, target_port: int, service_type: str) -> dict:\n    \"\"\"Build a service manifest.\"\"\"\n    return {\n        \"apiVersion\": \"v1\",\n        \"kind\": \"Service\",\n        \"metadata\": {\"name\": name},\n        \"spec\": {\n            \"type\": service_type,\n            \"ports\": [{\"port\": port, \"targetPort\": target_port}],\n            \"selector\": {\"app\": name}\n        }\n    }",
