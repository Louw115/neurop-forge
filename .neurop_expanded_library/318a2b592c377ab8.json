  "logic": "def format_e164(phone: str, country_code: str) -> str:\n    \"\"\"Format a phone number in E.164 format.\"\"\"\n    digits = normalize_phone(phone)\n    country = country_code.lstrip('+')\n    if digits.startswith(country):\n        return f\"+{digits}\"\n    return f\"+{country}{digits}\"",
