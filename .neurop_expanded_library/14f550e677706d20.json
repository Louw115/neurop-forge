  "logic": "def parse_semver(v0: str) -> dict:\n    import re\n    v1 = '^v?(\\\\d+)\\\\.(\\\\d+)\\\\.(\\\\d+)(?:-([a-zA-Z0-9.-]+))?(?:\\\\+([a-zA-Z0-9.-]+))?$'\n    v2 = v3.match(v1, v0)\n    if not v2:\n        return {'valid': False, 'major': 0, 'minor': 0, 'patch': 0}\n    return {'valid': True, 'major': int(v2.group(1)), 'minor': int(v2.group(2)), 'patch': int(v2.group(3)), 'prerelease': v2.group(4) or '', 'build': v2.group(5) or ''}",
