  "logic": "def apply_equal_jitter(delay: float, seed: int) -> float:\n    \"\"\"Apply equal jitter (delay/2 to delay).\"\"\"\n    h = hashlib.sha256(str(seed).encode()).digest()\n    return delay / 2 + delay / 2 * (h[0] / 255.0)",
