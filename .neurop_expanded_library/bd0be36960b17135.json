  "logic": "def build_metric_aggregation(values: list) -> dict:\n    \"\"\"Build metric aggregation summary.\"\"\"\n    if not values:\n        return {\"count\": 0, \"sum\": 0, \"min\": 0, \"max\": 0, \"avg\": 0}\n    return {\n        \"count\": len(values),\n        \"sum\": sum(values),\n        \"min\": min(values),\n        \"max\": max(values),\n        \"avg\": sum(values) / len(values)\n    }",
