  "logic": "def normalize_longitude(lon: float) -> float:\n    \"\"\"Normalize longitude to -180 to 180 range.\"\"\"\n    while lon > 180:\n        lon -= 360\n    while lon < -180:\n        lon += 360\n    return lon",
