  "logic": "def get_media_type(mime_type: str) -> str:\n    \"\"\"Get media type category.\"\"\"\n    if is_image_type(mime_type):\n        return \"image\"\n    if is_video_type(mime_type):\n        return \"video\"\n    if is_audio_type(mime_type):\n        return \"audio\"\n    if is_text_type(mime_type):\n        return \"text\"\n    if is_font_type(mime_type):\n        return \"font\"\n    return \"application\"",
