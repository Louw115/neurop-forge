  "logic": "def split_tax(total_tax: float, rates: list) -> list:\n    \"\"\"Split tax proportionally by rates.\"\"\"\n    total_rate = sum(rates)\n    if total_rate == 0:\n        return [0] * len(rates)\n    return [round(total_tax * rate / total_rate, 2) for rate in rates]",
