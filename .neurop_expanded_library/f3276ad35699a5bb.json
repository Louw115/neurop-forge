  "logic": "def get_extension(filename: str) -> str:\n    \"\"\"Extract the file extension from a filename.\"\"\"\n    if not filename:\n        return \"\"\n    if '.' not in filename:\n        return \"\"\n    return filename.rsplit('.', 1)[-1].lower()",
