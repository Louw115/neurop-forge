  "logic": "def get_analogous_colors(r: int, g: int, b: int) -> list:\n    \"\"\"Get analogous colors (adjacent on color wheel).\"\"\"\n    h, s, l = rgb_to_hsl(r, g, b)\n    return [\n        hsl_to_rgb((h - 30) % 360, s, l),\n        hsl_to_rgb(h, s, l),\n        hsl_to_rgb((h + 30) % 360, s, l),\n    ]",
