  "logic": "def create_link(href: str, text: str, new_tab: bool) -> str:\n    \"\"\"Create anchor link.\"\"\"\n    attrs = {\"href\": href}\n    if new_tab:\n        attrs[\"target\"] = \"_blank\"\n        attrs[\"rel\"] = \"noopener noreferrer\"\n    return create_element(\"a\", escape_html(text), attrs)",
