  "logic": "def get_default_port(scheme: str) -> int:\n    \"\"\"Get default port for scheme.\"\"\"\n    ports = {\n        \"http\": 80,\n        \"https\": 443,\n        \"ftp\": 21,\n        \"ssh\": 22,\n        \"telnet\": 23,\n        \"smtp\": 25,\n        \"dns\": 53,\n        \"pop3\": 110,\n        \"imap\": 143,\n        \"ldap\": 389,\n        \"smtps\": 465,\n        \"imaps\": 993,\n        \"pop3s\": 995,\n        \"mysql\": 3306,\n        \"postgresql\": 5432,\n        \"mongodb\": 27017,\n        \"redis\": 6379\n    }\n    return ports.get(scheme.lower(), 0)",
