  "logic": "def has_uppercase(value: str) -> dict:\n    \"\"\"Check if value has uppercase letter.\"\"\"\n    valid = any(c.isupper() for c in value)\n    return {\"valid\": valid, \"error\": \"Must contain uppercase letter\" if not valid else None}",
