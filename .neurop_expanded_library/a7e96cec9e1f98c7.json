  "logic": "def categorize_error(v0: str) -> str:\n    v1 = v0.lower()\n    if 'timeout' in v1:\n        return 'timeout'\n    if 'connection' in v1 or 'network' in v1:\n        return 'network'\n    if 'permission' in v1 or 'denied' in v1:\n        return 'permission'\n    if 'not found' in v1 or 'missing' in v1:\n        return 'not_found'\n    if 'invalid' in v1 or 'validation' in v1:\n        return 'validation'\n    if 'authentication' in v1 or 'unauthorized' in v1:\n        return 'auth'\n    return 'unknown'",
