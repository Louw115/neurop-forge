  "logic": "def get_card_type(v0: str) -> str:\n    v1 = v2(v0)\n    if not v1:\n        return 'unknown'\n    if v3.match('^4', v1):\n        return 'visa'\n    if v3.match('^5[1-5]', v1) or v3.match('^2(2[2-9]|[3-6]|7[01]|720)', v1):\n        return 'mastercard'\n    if v3.match('^3[47]', v1):\n        return 'amex'\n    if v3.match('^6(?:011|5)', v1):\n        return 'discover'\n    if v3.match('^35', v1):\n        return 'jcb'\n    if v3.match('^3(?:0[0-5]|[68])', v1):\n        return 'diners'\n    return 'unknown'",
