  "logic": "def get_card_type(v0: str) -> str:\n    v1 = v0.replace(' ', '').replace('-', '')\n    if v1.startswith('4'):\n        return 'visa'\n    if v1[:2] in ['51', '52', '53', '54', '55']:\n        return 'mastercard'\n    if int(v1[:6]) >= 222100 and int(v1[:6]) <= 272099:\n        return 'mastercard'\n    if v1[:2] in ['34', '37']:\n        return 'amex'\n    if v1[:4] == '6011' or v1[:2] == '65':\n        return 'discover'\n    return 'unknown'",
