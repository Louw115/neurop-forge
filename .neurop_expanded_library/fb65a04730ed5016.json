  "logic": "def calculate_next_occurrence(v0: dict, v1: int, v2: str) -> dict:\n    v3 = v0.get('days', [])\n    v4 = ['monday', 'tuesday', 'wednesday', 'thursday', 'friday', 'saturday', 'sunday']\n    for v5 in range(7):\n        v6 = (v1 + v5) % 7\n        if v4[v6] in [v7.lower() for v7 in v3]:\n            return {'day_offset': v5, 'day_name': v4[v6], 'time': v0.get('start_time')}\n    return {'day_offset': 0, 'day_name': v4[v1], 'time': v0.get('start_time')}",
