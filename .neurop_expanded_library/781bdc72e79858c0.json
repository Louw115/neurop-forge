  "logic": "def lru_put(cache: dict, key: str, value) -> dict:\n    \"\"\"Put value in cache (evicts LRU if full).\"\"\"\n    items = dict(cache[\"items\"])\n    order = list(cache[\"order\"])\n    if key in items:\n        order = [k for k in order if k != key]\n        items[key] = value\n        order.append(key)\n        return {\n            \"capacity\": cache[\"capacity\"],\n            \"items\": items,\n            \"order\": order,\n            \"size\": cache[\"size\"]\n        }\n    if len(items) >= cache[\"capacity\"]:\n        evicted = order.pop(0)\n        del items[evicted]\n    items[key] = value\n    order.append(key)\n    return {\n        \"capacity\": cache[\"capacity\"],\n        \"items\": items,\n        \"order\": order,\n        \"size\": len(items)\n    }",
