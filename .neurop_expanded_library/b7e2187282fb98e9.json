  "logic": "def is_valid_phone(phone: str) -> bool:\n    \"\"\"Check if a string is a valid phone number format.\"\"\"\n    if not phone:\n        return False\n    cleaned = re.sub(r'[\\s\\-\\.\\(\\)]', '', phone)\n    pattern = r'^\\+?[0-9]{10,15}$'\n    return bool(re.match(pattern, cleaned))",
