  "logic": "def generate_constraint_name(v0: str, v1: list, v2: str) -> str:\n    v3 = '_'.join(v1)\n    v4 = {'primary_key': 'pk', 'foreign_key': 'fk', 'unique': 'uq', 'check': 'ck'}\n    v5 = v4.get(v2, v2[:2])\n    return f'{v0}_{v3}_{v5}'",
