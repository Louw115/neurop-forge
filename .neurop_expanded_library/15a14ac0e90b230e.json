  "logic": "def atbash_cipher(text: str) -> str:\n    \"\"\"Apply Atbash cipher.\"\"\"\n    result = []\n    for c in text:\n        if \"a\" <= c <= \"z\":\n            result.append(chr(ord(\"z\") - (ord(c) - ord(\"a\"))))\n        elif \"A\" <= c <= \"Z\":\n            result.append(chr(ord(\"Z\") - (ord(c) - ord(\"A\"))))\n        else:\n            result.append(c)\n    return \"\".join(result)",
