  "logic": "def remove_item_from_order(order: dict, item_id: str) -> dict:\n    \"\"\"Remove item from order.\"\"\"\n    new_items = [i for i in order[\"items\"] if i.get(\"item_id\") != item_id]\n    return {**order, \"items\": new_items}",
