  "logic": "def generate_lock_table(v0: str, v1: str) -> str:\n    from neurop_forge.sources.sql_building import escape_sql_identifier\n    v2 = {'ACCESS SHARE', 'ROW SHARE', 'ROW EXCLUSIVE', 'SHARE UPDATE EXCLUSIVE', 'SHARE', 'SHARE ROW EXCLUSIVE', 'EXCLUSIVE', 'ACCESS EXCLUSIVE'}\n    v3 = v1.upper()\n    if v3 not in v2:\n        v3 = 'ACCESS SHARE'\n    return f'LOCK TABLE {v4(v0)} IN {v3} MODE'",
