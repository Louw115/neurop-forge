  "logic": "def format_as_table(v0: list, v1: list) -> str:\n    v2 = ''.join((f'<th>{v3}</th>' for v3 in v1))\n    v4 = ''\n    for v5 in v0:\n        v6 = ''.join((f'<td>{v7}</td>' for v7 in v5))\n        v4 += f'<tr>{v6}</tr>'\n    return f'<table><thead><tr>{v2}</tr></thead><tbody>{v4}</tbody></table>'",
