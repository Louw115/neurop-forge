  "logic": "def validate_phone_format(v0: str) -> dict:\n    import re\n    v1 = v2.sub('[\\\\s\\\\-\\\\(\\\\)]', '', v0)\n    if not v2.match('^\\\\+?[0-9]{7,15}$', v1):\n        return {'valid': False, 'error': 'Invalid phone number format'}\n    return {'valid': True, 'error': None}",
