  "logic": "def parse_signature_header(v0: str) -> dict:\n    v1 = {'version': '', 'algorithm': '', 'signature': ''}\n    v2 = v0.split('=')\n    if len(v2) == 3:\n        v1['version'] = v2[0].lstrip('v')\n        v1['algorithm'] = v2[1]\n        v1['signature'] = v2[2]\n    elif len(v2) == 2:\n        v1['algorithm'] = v2[0]\n        v1['signature'] = v2[1]\n    return v1",
