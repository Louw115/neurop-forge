  "logic": "def validate_state_machine(v0: dict) -> dict:\n    v1 = []\n    v2 = set(v0.get('states', []))\n    v3 = v0.get('initial_state', '')\n    if not v3:\n        v1.append('Missing initial state')\n    elif v3 not in v2:\n        v1.append(f\"Initial state '{v3}' not in states list\")\n    v4 = v0.get('transitions', {})\n    for v5, v6 in v4.items():\n        if v5 not in v2:\n            v1.append(f'Transition from unknown state: {v5}')\n        for v7 in v6:\n            if v7 not in v2:\n                v1.append(f'Transition to unknown state: {v7}')\n    return {'valid': len(v1) == 0, 'errors': v1}",
