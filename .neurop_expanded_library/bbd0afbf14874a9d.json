  "logic": "def validate_field_types(v0: dict, v1: dict) -> dict:\n    v2 = {'string': str, 'integer': int, 'number': (int, float), 'boolean': bool, 'array': list, 'object': dict}\n    v3 = []\n    for v4, v5 in v1.items():\n        if v4 in v0 and v0[v4] is not None:\n            v6 = v2.get(v5)\n            if v6 and (not isinstance(v0[v4], v6)):\n                v3.append(f'{v4} must be {v5}')\n    return {'valid': len(v3) == 0, 'errors': v3}",
