  "logic": "def json_type(value) -> str:\n    \"\"\"Get JSON type of value.\"\"\"\n    if value is None:\n        return \"null\"\n    if isinstance(value, bool):\n        return \"boolean\"\n    if isinstance(value, int) or isinstance(value, float):\n        return \"number\"\n    if isinstance(value, str):\n        return \"string\"\n    if isinstance(value, list):\n        return \"array\"\n    if isinstance(value, dict):\n        return \"object\"\n    return \"unknown\"",
