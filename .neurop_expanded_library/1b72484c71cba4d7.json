  "logic": "def add_role(user: dict, role: str) -> dict:\n    \"\"\"Add role to user.\"\"\"\n    if role in user.get(\"roles\", []):\n        return user\n    new_roles = user.get(\"roles\", []) + [role]\n    return {**user, \"roles\": new_roles}",
