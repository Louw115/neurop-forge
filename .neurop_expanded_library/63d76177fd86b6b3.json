  "logic": "def get_quadrant(lat: float, lon: float) -> str:\n    \"\"\"Get the geographic quadrant (NE, NW, SE, SW).\"\"\"\n    ns = 'N' if lat >= 0 else 'S'\n    ew = 'E' if lon >= 0 else 'W'\n    return ns + ew",
