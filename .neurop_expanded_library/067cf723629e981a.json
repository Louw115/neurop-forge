  "logic": "def remove_order_by(query: str) -> str:\n    \"\"\"Remove ORDER BY clause from a query.\"\"\"\n    import re\n    return re.sub(r'\\s+ORDER\\s+BY\\s+[^;]*?(?=\\s+LIMIT|\\s+OFFSET|\\s+FOR|\\s*;|\\s*$)', '', query, flags=re.IGNORECASE)",
