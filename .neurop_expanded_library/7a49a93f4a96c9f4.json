  "logic": "def is_valid_us_phone(phone: str) -> bool:\n    \"\"\"Check if a phone number is a valid US format.\"\"\"\n    digits = normalize_phone(phone)\n    if len(digits) == 10:\n        return digits[0] not in '01'\n    if len(digits) == 11 and digits[0] == '1':\n        return digits[1] not in '01'\n    return False",
