  "logic": "def close_tag(v0: dict, v1: str) -> dict:\n    v2 = '  ' * max(0, v0.get('indent', 1) - 1)\n    v3 = f'{v2}</{v1}>\\n'\n    return {'parts': v0['parts'] + [v3], 'length': v0['length'] + len(v3), 'indent': max(0, v0.get('indent', 1) - 1)}",
