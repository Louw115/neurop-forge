  "logic": "def desaturate(r: int, g: int, b: int, amount: float) -> dict:\n    \"\"\"Decrease saturation.\"\"\"\n    hsl = rgb_to_hsl(r, g, b)\n    new_s = max(0, hsl[\"s\"] - amount * 100)\n    return hsl_to_rgb(hsl[\"h\"], round(new_s), hsl[\"l\"])",
