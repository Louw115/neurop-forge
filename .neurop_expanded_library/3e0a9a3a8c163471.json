  "logic": "def extract_phones_from_text(v0: str) -> list:\n    v1 = ['\\\\+\\\\d{1,3}[-.\\\\s]?\\\\(?\\\\d{1,4}\\\\)?[-.\\\\s]?\\\\d{1,4}[-.\\\\s]?\\\\d{1,9}', '\\\\(?\\\\d{3}\\\\)?[-.\\\\s]?\\\\d{3}[-.\\\\s]?\\\\d{4}', '\\\\d{10,11}']\n    v2 = []\n    for v3 in v1:\n        v4 = v5.findall(v3, v0)\n        v2.extend(v4)\n    return list(set(v2))",
