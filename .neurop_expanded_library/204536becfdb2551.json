  "logic": "def get_out_degree(graph: dict, node_id: str) -> int:\n    \"\"\"Get out-degree of a node (for directed graphs).\"\"\"\n    return sum(1 for e in graph[\"edges\"] if e[\"from\"] == node_id)",
