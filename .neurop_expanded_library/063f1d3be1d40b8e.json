  "logic": "def slugify_preserve_unicode(text: str) -> str:\n    \"\"\"Slugify preserving unicode characters.\"\"\"\n    text = text.lower()\n    text = re.sub(r\"[^\\w\\s-]\", \"\", text, flags=re.UNICODE)\n    text = re.sub(r\"[\\s_]+\", \"-\", text)\n    text = re.sub(r\"-+\", \"-\", text)\n    return text.strip(\"-\")",
