  "logic": "def parse_cookie(header: str) -> dict:\n    \"\"\"Parse Cookie header.\"\"\"\n    cookies = {}\n    for pair in header.split(\";\"):\n        pair = pair.strip()\n        if \"=\" in pair:\n            key, value = pair.split(\"=\", 1)\n            cookies[key.strip()] = value.strip()\n    return cookies",
