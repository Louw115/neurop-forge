  "logic": "def url_encode(text: str) -> str:\n    \"\"\"URL-encode a string.\"\"\"\n    if not text:\n        return \"\"\n    result = []\n    for char in text:\n        if char.isalnum() or char in '-_.~':\n            result.append(char)\n        elif char == ' ':\n            result.append('+')\n        else:\n            encoded = char.encode('utf-8')\n            for byte in encoded:\n                result.append(f'%{byte:02X}')\n    return ''.join(result)",
