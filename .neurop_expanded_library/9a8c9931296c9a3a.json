  "logic": "def build_rest_endpoint(base_path: str, resource: str, resource_id: str, action: str) -> str:\n    \"\"\"Build a RESTful endpoint path.\"\"\"\n    path = base_path.rstrip('/')\n    if resource:\n        path += f\"/{resource}\"\n    if resource_id:\n        path += f\"/{resource_id}\"\n    if action:\n        path += f\"/{action}\"\n    return path",
