  "logic": "def extended_gcd(a: int, b: int) -> dict:\n    \"\"\"Extended Euclidean algorithm.\"\"\"\n    if b == 0:\n        return {\"gcd\": a, \"x\": 1, \"y\": 0}\n    result = extended_gcd(b, a % b)\n    return {\"gcd\": result[\"gcd\"], \"x\": result[\"y\"], \"y\": result[\"x\"] - (a // b) * result[\"y\"]}",
