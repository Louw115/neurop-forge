  "logic": "def to_snake_case(name: str) -> str:\n    \"\"\"Convert a string to snake_case.\"\"\"\n    import re\n    s1 = re.sub('(.)([A-Z][a-z]+)', r'\\1_\\2', name)\n    return re.sub('([a-z0-9])([A-Z])', r'\\1_\\2', s1).lower()",
