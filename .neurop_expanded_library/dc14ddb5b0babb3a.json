  "logic": "def line_slope(line: dict) -> float:\n    \"\"\"Calculate line slope.\"\"\"\n    dx = line[\"p2\"][\"x\"] - line[\"p1\"][\"x\"]\n    if dx == 0:\n        return float(\"inf\")\n    return (line[\"p2\"][\"y\"] - line[\"p1\"][\"y\"]) / dx",
