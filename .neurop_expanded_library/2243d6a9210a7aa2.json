  "logic": "def parse_cidr(v0: str) -> dict:\n    if '/' not in v0:\n        return {'network': v0, 'prefix': 32, 'valid': v1(v0)}\n    v2 = v0.split('/')\n    v3 = v2[0]\n    v4 = int(v2[1])\n    return {'network': v3, 'prefix': v4, 'valid': v1(v3) and 0 <= v4 <= 32}",
