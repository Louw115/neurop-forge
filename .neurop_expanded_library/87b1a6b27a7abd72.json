  "logic": "def has_path(data, path: str) -> bool:\n    \"\"\"Check if path exists.\"\"\"\n    keys = path.split(\".\")\n    current = data\n    for key in keys:\n        if isinstance(current, dict) and key in current:\n            current = current[key]\n        elif isinstance(current, list):\n            try:\n                current = current[int(key)]\n            except (ValueError, IndexError):\n                return False\n        else:\n            return False\n    return True",
