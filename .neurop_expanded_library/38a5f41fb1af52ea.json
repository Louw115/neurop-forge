  "logic": "def get_throttle_stats(v0: dict, v1: int) -> dict:\n    v2 = v1 - v0['window_ms']\n    v3 = [v4 for v4 in v0['calls'] if v4 >= v2]\n    return {'current_calls': len(v3), 'max_calls': v0['max_calls'], 'remaining': max(0, v0['max_calls'] - len(v3)), 'blocked_total': v0['blocked'], 'utilization': len(v3) / v0['max_calls'] if v0['max_calls'] > 0 else 0}",
