  "logic": "def is_valid_date(year: int, month: int, day: int) -> bool:\n    \"\"\"Check if a date is valid.\"\"\"\n    if month < 1 or month > 12:\n        return False\n    if day < 1:\n        return False\n    return day <= days_in_month(year, month)",
