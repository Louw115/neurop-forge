  "logic": "def parse_association_key(key: str) -> dict:\n    \"\"\"Parse an association key into components.\"\"\"\n    parts = key.split(\":\")\n    if len(parts) == 4:\n        return {\n            \"from_type\": parts[0],\n            \"from_id\": parts[1],\n            \"to_type\": parts[2],\n            \"to_id\": parts[3]\n        }\n    return {}",
