  "logic": "def has_special_char(value: str) -> dict:\n    \"\"\"Check if value has special character.\"\"\"\n    special = set(\"!@#$%^&*()_+-=[]{}|;':\\\",./<>?\")\n    valid = any(c in special for c in value)\n    return {\"valid\": valid, \"error\": \"Must contain a special character\" if not valid else None}",
