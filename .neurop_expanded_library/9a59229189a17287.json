  "logic": "def calculate_next_reminder_time(v0: str, v1: str) -> str:\n    from datetime import datetime, timedelta\n    v2 = v3.fromisoformat(v0.replace('Z', '+00:00'))\n    v4 = {'daily': v5(days=1), 'weekly': v5(weeks=1), 'monthly': v5(days=30), 'yearly': v5(days=365)}\n    v6 = v4.get(v1, v5(days=1))\n    return (v2 + v6).isoformat()",
