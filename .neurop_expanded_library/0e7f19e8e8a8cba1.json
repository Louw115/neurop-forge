  "logic": "def mask_name(name: str) -> str:\n    \"\"\"Mask name showing only initials.\"\"\"\n    words = name.split()\n    return \" \".join(w[0] + \"*\" * (len(w) - 1) if len(w) > 1 else w for w in words)",
