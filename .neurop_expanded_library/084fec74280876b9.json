  "logic": "def triangle_area(p1: dict, p2: dict, p3: dict) -> float:\n    \"\"\"Calculate triangle area.\"\"\"\n    return abs((p2[\"x\"] - p1[\"x\"]) * (p3[\"y\"] - p1[\"y\"]) -\n               (p3[\"x\"] - p1[\"x\"]) * (p2[\"y\"] - p1[\"y\"])) / 2",
