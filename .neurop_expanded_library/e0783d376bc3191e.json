  "logic": "def parse_display_email(v0: str) -> dict:\n    v1 = v2.match('^(?:\"?([^\"]*)\"?\\\\s+)?<?([^<>]+@[^<>]+)>?$', v0.strip())\n    if v1:\n        return {'name': v1.group(1) or '', 'email': v1.group(2)}\n    return {'name': '', 'email': v0}",
