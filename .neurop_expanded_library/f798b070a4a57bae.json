  "logic": "def vector_cross_product_3d(v1: dict, v2: dict) -> dict:\n    \"\"\"Calculate cross product of two 3D vectors.\"\"\"\n    a = v1[\"values\"]\n    b = v2[\"values\"]\n    return create_vector([\n        a[1] * b[2] - a[2] * b[1],\n        a[2] * b[0] - a[0] * b[2],\n        a[0] * b[1] - a[1] * b[0]\n    ])",
